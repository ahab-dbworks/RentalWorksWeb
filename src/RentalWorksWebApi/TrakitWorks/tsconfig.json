{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2015",
    "moduleResolution": "Node"
  },
  "include": [
    "./@types/**/*.d.ts",
    "./Libraries/**/*.ts",
    "./scripts/**/*.ts",
    "./Source/**/*.ts",
    "./DefaultApplicationConfig.ts"
  ]
}
