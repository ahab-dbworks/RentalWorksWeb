<!--Order Report-->
<div class="rpt-flexrow" style="margin:5px 0px 0px 4px;"> 
  <div class="rpt-flexcolumn" style="border-bottom: 3px solid black;padding-bottom:4px;">
    <span class="header-left" style="font-weight:600">{{Company}}</span>
    <span class="header-left">{{OfficeLocationAddress1}}</span>
    <span class="header-left">{{OfficeLocationAddress2}}</span>
    <span class="header-left">{{OfficeLocationCityStateZipCodeCountry}}</span>
    <span class="header-left"><span style="font-weight:600;">Phone:</span> {{OfficeLocationPhone}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="title">{{Report}}</span>
    <span class="header-center">Order Date: {{OrderDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="header-right" style="font-size:30px;">Order No: <span style="border:1px solid black;padding:2px;">{{OrderNumber}}</span></span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  <table class="sub-header">
    <tr>
      <td class="header-left"><span style="font-weight:600;padding-right:40px;">Agent:</span> {{Agent}}</td>
      <td class="header-center"><span style="font-weight:600;">Description:</span> {{Description}}</td>
      <td class="header-right"><span style="font-weight:600;">Billing Period:</span> {{BillingDates}}</td>
    </tr>
    <tr>
      <td class="header-left"><span style="font-weight:600;padding-right:44px;">Email:</span> {{AgentEmail}}</td>
      <td class="header-center"><span style="font-weight:600;padding-right:13px;">Customer:</span> {{Customer}}</td>
      <td class="header-right"><span style="font-weight:600;padding-right:51px;">Terms:</span> {{PaymentTerms}}</td>
    </tr>
    <tr>
      <td class="header-left"><span style="font-weight:600;">Phone / Ext:</span> {{AgentPhoneAndExtension}}</td>
      <td class="header-center"><span style="font-weight:600;padding-right:37px;">PO No:</span> {{PoNumber}}</td>
      <td class="header-right"><span style="font-weight:600;">Usage Period:</span> {{UsageDates}}</td>
    </tr>
    <tr>
      <td class="header-left"><span style="font-weight:600;padding-right:50px;">Deal:</span> {{Deal}}</td>
      <td class="header-center"><span style="font-weight:600;padding-right:27px;">Deal No:</span> {{DealNumber}}</td>
      <td class="header-right"><span style="font-weight:600;padding-right:7px;">Billing Cycle:</span> {{BillingCycle}}</td>
    </tr>
  </table>
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  <table>
    <thead>
      <tr>
        <th class="text" style="text-align:left;">Issued To</th>
        <th class="text" style="text-align:left;padding-left:78px;">Outgoing (DELIVER)</th>
        <th class="text" style="text-align:left;padding-left:27px;">Incoming (DELIVER)</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td id="issuedTo" class="text" style="text-align:left;">
          <div>
            {{IssuedToCompany}}
          </div>
          <div>
            {{IssuedToAttention1}}
          </div>
          <div>
            {{IssuedToAttention2}}
          </div>
          <div>
            {{IssuedToAddress1}}
          </div>
          <div>
            {{IssuedToAddress2}}
          </div>
          <div>
            {{IssuedToCity}}, {{IssuedToState}} {{IssuedToZipCode}} {{IssuedToCountry}}
          </div>
          <div>
            Phone: {{IssuedToPhone}}
          </div>
          <div>
            Fax: {{IssuedToFax}}
          </div>
        </td>
        <td id="outgoing" class="text" style="text-align:left;padding-left:78px;">
          <div>
            Attention: {{OutDeliveryAttention}}
          </div>
          <div>
            {{OutDeliveryAddress1}}
          </div>
          <div>
            {{OutDeliveryAddress2}}
          </div>
          <div>
            {{OutDeliveryCityStateZipCodeCountry}}
          </div>
          <div>
            Phone: {{OutDeliveryContactPhone}}
          </div>
          <div>
            Required By: {{OutDeliveryRequiredByDateTime}}
          </div>
        </td>
        <td id="incoming" class="text" style="text-align:left;padding-left:27px;">
          <div>
            Attention: {{InDeliveryAttention}}
          </div>
          <div>
            {{InDeliveryAddress1}}
          </div>
          <div>
            {{InDeliveryAddress2}}
          </div>
          <div>
            {{InDeliveryCityStateZipCodeCountry}}
          </div>
          <div>
            Contact: {{InDeliveryContact}}
          </div>
          <div>
            Phone: {{InDeliveryContactPhone}}
          </div>
          <div>
            Required By: {{InDeliveryRequiredByDateTime}}
          </div>
        </td>
      </tr>
    </tbody>
  </table>
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  <table>
    <thead>
      <tr>
        <th class="text">I-Code</th>
        <th class="text">Description</th>
        <th class="number">Quantity</th>
        <th class="number">Period Extended</th>
      </tr>
    </thead>
    <tbody>
      {{#each Items}}
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "RecTypeDisplayheader"}}
        <td colspan="8" style="font-weight:bold;text-align:left;padding-top:11px">{{RecTypeDisplay}}&nbsp;</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="text">{{ICode}}</td>
        <td class="text">{{Description}}</td>
        <td class="number">{{QuantityOrdered}}</td>
        <td class="number">{{PeriodExtended}}</td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>