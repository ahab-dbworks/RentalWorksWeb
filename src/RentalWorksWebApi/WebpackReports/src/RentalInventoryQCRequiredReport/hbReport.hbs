<!--RentalInventoryQCRequiredReport-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">As Of: {{Today}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    <thead>
      <tr>
        <th>I-Code</th>
        <th>Description</th>
        <th>Bar Code</th>
        <th>In Contract No.</th>
        <th>In Date</th>
        <th>Deal</th>
        <th>Condition</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "InventoryTypeheader"}}
        <td colspan="5" class="header-row">Inventory Type: {{InventoryType}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryheader"}}
        <td colspan="5" class="header-row">Category: {{"Category"}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="nowrap" data-value="{{ICode}}"></td>
        <td data-value="{{Description}}"></td>
        <td data-value="{{BarCode}}"></td>
        <td data-value="{{InContractNumber}}"></td>
        <td data-value="{{InContractDate}}"></td>
        <td data-value="{{LastDeal}}"></td>
        <td data-value="{{Condition}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>