<!--RentalInventoryChangeReport-->
<div class="rpt-flexrow-ROA">
  <span class="rpt-title">{{Report}}</span>
</div>
<div class="rpt-flexrow-ROA" style="margin-top:40px;">
  <table>
    <thead>
      <tr>
        <th class="text">Warehouse</th>
        <th class="text">Inventory Type</th>
        <th class="text">Category</th>
        <th class="text">Sub Category</th>
        <th class="text">ICode Rank</th>
        <th class="text">ICode</th>
        <th class="text">Description</th>
        <th class="text">Trans Date</th>
        <th class="text">Trans Type</th>
        <th class="text">Change Description</th>
        <th class="number">Addition Qty</th>
        <th class="number">Unit Price Addition</th>
        <th class="number">Ext Addition Price</th>
        <th class="number">Subtraction Qty</th>
        <th class="number">Unit Price Subtraction</th>
        <th class="number">Ext Subtraction Price</th>
        <th class="number">Running Total Qty</th>
        <th class="number">Running Total Value</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr>
        <td class="text" data-value="{{Warehouse}}"></td>
        <td class="text" data-value="{{InventoryType}}"></td>
        <td class="text" data-value="{{Category}}"></td>
        <td class="text" data-value="{{SubCategory}}"></td>
        <td class="text" data-value="{{ICodeRank}}"></td>
        <td class="text" data-value="{{ICode}}"></td>
        <td class="text" data-value="{{ICodeDescription}}"></td>
        <td class="text" data-value="{{TransactionDate}}"></td>
        <td class="text" data-value="{{TransactionType}}"></td>
        <td class="text" data-value="{{ChangeDescription}}"></td>
        <td class="number" data-value="{{AdditionQuantity}}"></td>
        <td class="number" data-value="{{UnitPriceAddition}}"></td>
        <td class="number" data-value="{{ExtendedAdditionPrice}}"></td>
        <td class="number" data-value="{{SubtractionQuantity}}"></td>
        <td class="number" data-value="{{UnitPriceSubtraction}}"></td>
        <td class="number" data-value="{{ExtendedSubtractionPrice}}"></td>
        <td class="number" data-value="{{RunningTotalQuantity}}"></td>
        <td class="number" data-value="{{RunningTotalValue}}"></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
