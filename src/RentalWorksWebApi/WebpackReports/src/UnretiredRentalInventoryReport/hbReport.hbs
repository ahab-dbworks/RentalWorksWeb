<!--Unretired Rental Inventory Report-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
       <span class="rpt-header-center">Unretired From: {{FromDate}} To: {{ToDate}}</span>
    </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    <thead>
      <tr>
        <th class="text">I-Code</th>
        <th class="text">Description</th>
        <th class="number">Quantity</th>
        <th class="text">Bar Code</th>
        <th class="text">Retired Date</th>
        <th class="text">Retired By</th>
        <th class="text">Retired Reason</th>
        <th class="text">Un-Retired Date</th>
        <th class="text">Un-Retired By</th>
        <th class="text">Un-Retired Reason</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr class="detail-view-row" data-rowtype={{RowType}}>
        {{#ifEquals RowType "Warehouseheader"}}
        <td colspan="5" class="header-row">Warehouse: {{Warehouse}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypeheader"}}
        <td colspan="5" class="lower header-row">Inventory Type: {{"InventoryType"}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="text" data-value="{{ICode}}"></td>
        <td class="text" data-value="{{Description}}"></td>
        <td class="number" data-value="{{Quantity}}"></td>
        <td class="text" data-value="{{BarCode}}"></td>
        <td class="text" data-value="{{RetireDate}}"></td>
        <td class="text" data-value="{{RetiredBy}}"></td>
        <td class="text" data-value="{{RetiredReason}}"></td>
        <td class="text" data-value="{{UnretireDate}}"></td>
        <td class="text" data-value="{{UnretiredBy}}"></td>
        <td class="text" data-value="{{UnretiredReason}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypefooter"}}
        <td colspan="2" style="font-weight:bold;text-align:right;padding-top:11px">{{InventoryType}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Warehousefooter"}}
        <td colspan="2" style="font-weight:bold;text-align:right;padding-top:11px">{{Warehouse}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "grandtotal"}}
        <td colspan="2" style="font-weight:bold;text-align:right;padding-top:11px">Grand Total:&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>