<!--Sales Inventory Transaction Report-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    <thead>
      <tr>
        <th class="text">Warehouse</th>
        <th class="text">I-Code</th>
        <th class="text">Description</th>
        <th class="text">Trans Date</th>
        <th class="text">User</th>
        <th class="text">Trans Type</th>
        <th class="text">Trans No.</th>
        <th class="number">Quantity</th>
        <th class="number">Cost</th>
        <th class="number">Cost Extended</th>
        <th class="number">Sell Price</th>
        <th class="number">Sell Extended</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr>
        <td class="text" data-value="{{WarehouseCode}}"></td>
        <td class="text" data-value="{{ICode}}"></td>
        <td class="text" data-value="{{Description}}"></td>
        <td class="text" data-value="{{TransactionDate}}"></td>
        <td class="text" data-value="{{UsersName}}"></td>
        <td class="text" data-value="{{TransactionType}}"></td>
        <td class="text" data-value="{{TransactionNumber}}"></td>
        <td class="text" data-value="{{QuantityOrdered}}"></td>
        <td class="number" data-value="{{UnitCost}}"></td>
        <td class="number" data-value="{{CostExtended}}"></td>
        <td class="number" data-value="{{UnitPrice}}"></td>
        <td class="number" data-value="{{PriceExtended}}"></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>