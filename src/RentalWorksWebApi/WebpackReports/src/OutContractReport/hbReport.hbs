<!--OutContract Report-->
<div class="rpt-flexrow" style="margin:5px 0px 0px 4px;">
  <div class="rpt-flexcolumn" style="padding-bottom:5px;">
    {{#if Logosrc}}
    <div style="height:130px;"><img class="clientlogo" src="{{Logosrc}}" style="display:block;width:auto;height:100%;" alt="logo" /></div>
    {{else}}
    <div></div>
    {{/if}}
    <span style="font-weight:600; font-size:18px;">{{Company}}</span>
    <span class="company-header">{{LocationAddress1}}</span>
    <span class="company-header">{{LocationAddress2}}</span>
    <span class="company-header">{{LocationCity}}, {{LocationStateOrProvince}} {{LocationZipCode}}</span>
    <span class="company-header"><span style="font-weight:600;">Phone: </span>{{LocationPhone}}</span>
    {{#gt LocationFax.length 0}}
    <span class="company-header"><span style="font-weight:600;">Fax: </span>{{LocationFax}}</span>
    {{/gt}}
  </div>
  <div style="max-width: 200px;" class="rpt-flexcolumn">
    <span class="sub-title blue">{{Department}}</span>
    <span class="contract-title blue">{{Report}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="header-right blue" style="font-size:32px;"><span style="font-weight:600;padding-right:8px;">No: </span><span style="background-color:#DCDCDC;color:black;border:3px solid #0798ee;padding:3px;">{{ContractNumber}}</span></span>
    <span style="font-size:20px;padding-top:10px;padding-right:101px;" class="header-right blue"><span style="font-weight:600;">Date: </span><span style="color:black;padding-left:12px;">{{ContractDate}}</span></span>
    <span style="font-size:20px;padding-top:3px;padding-right:124px;" class="header-right blue"><span style="font-weight:600;">Time: </span><span style="color:black;padding-left:12px;">{{ContractTime}}</span></span>
  </div>
</div>
<div class="rpt-flexrow" style="padding-left:3px;">
  <div class="rpt-flexcolumn">
    <div class="meta-header"><span>Issued To:</span></div>
    <div class="meta-address">
      <span class="header-left">{{Deal}}</span>
      {{#if BillToAttention}}
      <span class="header-left">c/o {{BillToAttention}}</span>
      {{/if}}
      <span class="header-left">{{DealAddress1}}</span>
      <span class="header-left">{{DealAddress2}}</span>
      <span class="header-left">{{DealCity}}, {{DealStateOrProvince}} {{DealZipCode}}</span>
      <span class="header-left"><span style="font-weight:600;">Phone: </span>{{DealPhone}}</span>
      {{#gt DealFax.length 0}}
      <span class="header-left"><span style="font-weight:600;">Fax: </span>{{DealFax}}</span>
      {{/gt}}
    </div>
  </div>
  <div style="max-width:50px;" class="rpt-flexcolumn middle">
    <span style="padding:10px;"></span>
  </div>
  <div class="rpt-flexcolumn">

    <div class="meta-header"><span>Order Details:</span></div>
    <table style="border-top:none;" class="meta-section">
      <tr>
        <td style="width:84px;" class="shaded-block">Order No: </td>
        <td class="meta-content" style="vertical-align:top;">{{OrderNumber}}</td>
      </tr>
      <tr>
        <td style="width:84px;" class="shaded-block">Deal: </td>
        <td class="meta-content" style="vertical-align:top;">{{Deal}}</td>
      </tr>
      <tr>
        <td style="width:84px;" class="shaded-block">Description: </td>
        <td class="meta-content" style="vertical-align:top;">{{OrderDescription}}</td>
      </tr>
    </table>
    <table class="meta-section">
      <tr>
        <td style="width:84px;" class="shaded-block">PO No: </td>
        <td class="meta-content">{{PoNumber}}</td>
      </tr>
      <tr>
        <td style="width:84px;" class="shaded-block">Terms: </td>
        <td class="meta-content">{{PaymentTerms}}</td>
      </tr>
      <tr>
        <td style="width:84px;" class="shaded-block">Billing Cycle: </td>
        <td class="meta-content">{{BillingCycle}}</td>
      </tr>
    </table>
    <table class="meta-section">
      <tr>
        <td style="width:84px;" class="shaded-block">Usage Dates: </td>
        <td class="meta-content">{{EstimatedStartDate}} - {{EstimatedEndDate}}</td>
      </tr>
      <tr>
        <td style="width:84px;" class="shaded-block">Billing Dates: </td>
        <td class="meta-content">{{BillingStartDate}} - {{BillingEndDate}}</td>
      </tr>
    </table>
    <table class="meta-section">
      <tr>
        <td style="width:84px;" class="shaded-block">Agent: </td>
        <td class="meta-content">{{Agent}}</td>
      </tr>
      <tr>
        <td style="width:84px;" class="shaded-block">Email: </td>
        <td class="meta-content">{{AgentEmail}}</td>
      </tr>
    </table>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  {{#gt RentalItems.length 0}}
  <table>
    <thead>
      <tr>
        <td colspan="8" class="section-header">RENTAL&nbsp;</td>
      </tr>
      <tr>
        <td></td>
        <th>I-Code</th>
        <th>Description</th>
        <th class="number">Ordered</th>
        <th class="number">Out</th>
        <th class="number">Total Out</th>
      </tr>
    </thead>
    <tbody>
      {{#each RentalItems}}
      <tr class="item-row">
        <td style="background-color:white;"></td>
        <td>{{ICode}}</td>
        <td>{{Description}}</td>
        <td class="number">{{QuantityOrdered}}</td>
        <td class="number">{{QuantityOut}}</td>
        <td class="number">{{TotalOut}}</td>
      </tr>
      {{#gt Barcode.length 0}}
      <tr>
        <td colspan="5" class="barcodes"><span>Barcodes:</span> {{Barcode}}</td>
      </tr>
      {{/gt}}
      {{/each}}
    </tbody>
  </table>
  {{/gt}}

{{#gt SalesItems.length 0}}
<table>
  <thead>
    <tr>
      <td colspan="8" class="section-header">SALES&nbsp;</td>
    </tr>
    <tr>
      <td style="background-color:white;"></td>
      <th>I-Code</th>
      <th>Description</th>
      <th class="number">Ordered</th>
      <th class="number">Out</th>
      <th class="number">Total Out</th>
    </tr>
  </thead>
  <tbody>
    {{#each SalesItems}}
    <tr class="item-row">
      <td></td>
      <td>{{ICode}}</td>
      <td>{{Description}}</td>
      <td class="number">{{QuantityOrdered}}</td>
      <td class="number">{{QuantityOut}}</td>
      <td class="number">{{TotalOut}}</td>
    </tr>
    {{#gt Barcode.length 0}}
    <tr>
      <td colspan="5" class="barcodes"><span>Barcodes:</span> {{Barcode}}</td>
    </tr>
    {{/gt}}
    {{/each}}
  </tbody>
</table>
{{/gt}}
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  <fieldset>
    <legend>RECEIVED BY</legend>
    <div>
      <div style="display:flex">
        <div class="receivedbyfield">
          <div class="value"><img src="{{ReceivedBySignatureDataUrl}}" /></div>
          <div class="caption">SIGNATURE</div>
        </div>
        <div class="receivedbyfield">
          <div class="value">{{ReceivedBySignature}}</div>
          <div class="caption">PRINT NAME</div>
        </div>
      </div>
      <div style="display:flex">
        <div class="receivedbyfield">
          <div class="value">{{ReceivedByDate}}</div>
          <div class="caption">DATE</div>
        </div>
        <div class="receivedbyfield"></div>
      </div>
    </div>
  </fieldset>
</div>
