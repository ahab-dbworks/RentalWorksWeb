<!--RentalInventoryAvailabilityReport-->
<div class="rpt-flexrow" style="width:1525px;">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">{{Today}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;width:1525px;">
  <table>
    <thead>
      <tr>
        <th>I-Code</th>
        <th>Description</th>
        <th class="number">Qty Owned</th>
        <th class="number">On Truck</th>
        <th class="number">Transfer Out</th>
        <th class="number">In Repair</th>
        <th class="number">Sub-Rent</th>
           <th class="number">Late</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "Warehouseheader"}}
        <td colspan="5" class="header-row">Warehouse: {{Warehouse}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypeheader"}}
        <td colspan="5" class="lower header-row">Inventory Type: {{InventoryType}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryheader"}}
        <td colspan="5" class="lower header-row">Category: {{"Category"}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="nowrap" data-value="{{ICode}}"></td>
        <td data-value="{{Description}}"></td>
        <td class="number" data-value="{{QuantityOwned}}"></td>
        <td class="number" data-value="{{QuantityStaged}}"></td>
        <td class="number" data-value="{{QuantityOut}}"></td>
        <td class="number" data-value="{{QuantityHolding}}"></td>
        <td class="number" data-value="{{QuantityAvailable}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryfooter"}}
        <td colspan="9" class="total-name">{{Category}}:&nbsp;</td>
        <td class="total-val" data-value="{{QuantityOrdered}}"></td>
        <td class="total-val" data-value="{{QuantityStaged}}"></td>
        <td class="total-val" data-value="{{QuantityOut}}"></td>
        <td class="total-val" data-value="{{QuantityHolding}}"></td>
        <td class="total-val" data-value="{{QuantityAvailable}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypefooter"}}
        <td colspan="9" class="total-name">{{InventoryType}}:&nbsp;</td>
        <td class="total-val" data-value="{{QuantityOrdered}}"></td>
        <td class="total-val" data-value="{{QuantityStaged}}"></td>
        <td class="total-val" data-value="{{QuantityOut}}"></td>
        <td class="total-val" data-value="{{QuantityHolding}}"></td>
        <td class="total-val" data-value="{{QuantityAvailable}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Warehousefooter"}}
        <td colspan="9" class="total-name">{{Warehouse}}:&nbsp;</td>
        <td class="total-val" data-value="{{QuantityOrdered}}"></td>
        <td class="total-val" data-value="{{QuantityStaged}}"></td>
        <td class="total-val" data-value="{{QuantityOut}}"></td>
        <td class="total-val" data-value="{{QuantityHolding}}"></td>
        <td class="total-val" data-value="{{QuantityAvailable}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "grandtotal"}}
        <td colspan="9" class="total-name">Grand Total:&nbsp;</td>
        <td class="total-val" data-value="{{QuantityOrdered}}"></td>
        <td class="total-val" data-value="{{QuantityStaged}}"></td>
        <td class="total-val" data-value="{{QuantityOut}}"></td>
        <td class="total-val" data-value="{{QuantityHolding}}"></td>
        <td class="total-val" data-value="{{QuantityAvailable}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>