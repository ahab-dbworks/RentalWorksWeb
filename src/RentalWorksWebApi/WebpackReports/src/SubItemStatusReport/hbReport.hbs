<!--Sub-Rental Billing Analysis Report-->
<div class="rpt-flexrow" style="width:1555px;">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;width:1555px;">
  <table style="border-top:2px solid black;">
    {{#each this}}
    {{#ifEquals RowType "OfficeLocationheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th class="header-row no-border" colspan="16">Office Location: {{OfficeLocation}}</th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "Departmentheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th class="lower header-row no-border" colspan="16">Department: {{Department}}</th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "OrderNumberheader"}}
    <thead>
      <tr style="padding-top:15px;" data-rowtype={{RowType}}>
        <th colspan="2" class="lower no-border">Order: {{OrderNumberAndDescription}}</th>
        <th colspan="9" class="line-header">Deal: <span>{{Deal}}</span></th>
        <th colspan="3" class="line-header">Order Date: <span>{{OrderDate}}</span></th>
        <th colspan="5" class="line-header">Period: <span>{{OrderEstimatedPeriod}}</span></th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "PurchaseOrderNumberheader"}}
    <thead>
      <tr style="padding-top:15px;" data-rowtype={{RowType}}>
        <th colspan="2" class="lower no-border header-row">PO: {{PurchaseOrderNumberAndDescription}}</th>
        <th colspan="5" class="line-header">Vendor: <span>{{Vendor}}</span></th>
        <th colspan="4" class="line-header">Class: <span>{{PurchaseOrderClassification}}</span></th>
        <th colspan="3" class="line-header">PO Date: <span>{{PurchaseOrderDate}}</span></th>
        <th colspan="5" class="line-header">Period: <span>{{PurchaseOrderEstimatedPeriod}}</span></th>
      </tr>
    </thead>
    <thead class="top-section-header">
      <tr>
        <th colspan="3" style="background-color:white;border-top:none;"></th>
        <th colspan="4" style="border-left:3px solid black; border-right:3px solid black;">VENDOR</th>
        <th colspan="4" style="border-right:3px solid black;">DEAL</th>
        <th colspan="5" style="background-color:white;border-top:none;"></th>
      </tr>
    </thead>
    <thead>
      <tr>
        <th>I-Code</th>
        <th>Description</th>
        <th class="number">Qty</th>
        <th style="width:55px;" class="number">Rate</th>
        <th style="width:55px;" class="number">Disc %</th>
        <th class="number">Weekly</th>
        <th class="number">Period</th>
        <th class="number">Rate</th>
        <th class="number">Disc %</th>
        <th class="number">Weekly</th>
        <th class="number">Period</th>
        <th class="number">Profit</th>
        <th>Receive</th>
        <th>Out</th>
        <th>In</th>
        <th>Return</th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "detail"}}
    <tbody data-rowtype="{{RowType}}">
      <tr class="detail">
        <td class="nowrap" data-value="{{ICode}}"></td>
        <td data-value="{{ItemDescription}}"></td>
        <td class="number" data-value="{{SubQuantity}}"></td>
        <td class="number" data-value="{{PurchaseOrderRate}}"></td>
        <td class="number" data-value="{{PurchaseOrderDiscountPercent}}"></td>
        <td class="number" data-value="{{PurchaseOrderWeeklyExtended}}"></td>
        <td class="number" data-value="{{PurchaseOrderPeriodExtended}}"></td>
        <td class="number" data-value="{{OrderRate}}"></td>
        <td class="number" data-value="{{OrderDiscountPercent}}"></td>
        <td class="number" data-value="{{OrderWeeklyExtended}}"></td>
        <td class="number" data-value="{{OrderPeriodExtended}}"></td>
        <td class="number" data-value="{{Profit}}"></td>
        <td data-value="{{ReceiveDate}}"></td>
        <td data-value="{{OutDate}}"></td>
        <td data-value="{{InDate}}"></td>
        <td data-value="{{ReturnDate}}"></td>
      </tr>
    </tbody>
    {{/ifEquals}}
    <tr data-rowtype="{{RowType}}">
      {{#ifEquals RowType "OfficeLocationfooter"}}
      <td colspan="2" class="total-name">{{OfficeLocation}}:&nbsp;</td>
      <td class="total-val" data-value="{{SubQuantity}}"></td>
      <td colspan="4" class="total-val" data-value="{{PurchaseOrderPeriodExtended}}"></td>
      <td colspan="4" class="total-val" data-value="{{OrderPeriodExtended}}"></td>
      <td class="total-val" data-value="{{Profit}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "Departmentfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="2" class="total-name">{{Department}}:&nbsp;</td>
      <td class="total-val" data-value="{{SubQuantity}}"></td>
      <td colspan="4" class="total-val" data-value="{{PurchaseOrderPeriodExtended}}"></td>
      <td colspan="4" class="total-val" data-value="{{OrderPeriodExtended}}"></td>
      <td class="total-val" data-value="{{Profit}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "OrderNumberfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="2" class="total-name">{{OrderNumber}}:&nbsp;</td>
      <td class="total-val" data-value="{{SubQuantity}}"></td>
      <td colspan="4" class="total-val" data-value="{{PurchaseOrderPeriodExtended}}"></td>
      <td colspan="4" class="total-val" data-value="{{OrderPeriodExtended}}"></td>
      <td class="total-val" data-value="{{Profit}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "PurchaseOrderNumberfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="2" class="total-name">{{PurchaseOrderNumber}}:&nbsp;</td>
      <td class="total-val" data-value="{{SubQuantity}}"></td>
      <td colspan="4" class="total-val" data-value="{{PurchaseOrderPeriodExtended}}"></td>
      <td colspan="4" class="total-val" data-value="{{OrderPeriodExtended}}"></td>
      <td class="total-val" data-value="{{Profit}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "grandtotal"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="2" class="total-name">Grand Total:&nbsp;</td>
      <td class="total-val" data-value="{{SubQuantity}}"></td>
      <td colspan="4" class="total-val" data-value="{{PurchaseOrderPeriodExtended}}"></td>
      <td colspan="4" class="total-val" data-value="{{OrderPeriodExtended}}"></td>
      <td class="total-val" data-value="{{Profit}}"></td>
    </tr>
    {{/ifEquals}}
    {{/each}}
  </table>
</div>