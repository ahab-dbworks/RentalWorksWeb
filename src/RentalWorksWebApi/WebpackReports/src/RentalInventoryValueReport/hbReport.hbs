<!--Rental Inventory Value Report-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
    <span class="rpt-header-center">{{ConsignedOwnedHeader}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <!--SUMMARY VIEW TABLE-->
  {{#ifEquals ViewSetting "SummaryView"}}
  <table>
    <thead>
      <tr>
        <th class="text">I-Code</th>
        <th class="text">Description</th>
        <th class="text">Date</th>
        <th class="number">Quantity</th>
        <th class="number">Avg. Unit Value</th>
        <th class="number">Extended</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "Warehouseheader"}}
        <td colspan="5" class="header-row">Warehouse: {{Warehouse}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypeheader"}}
        <td colspan="5" class="lower header-row">Inventory Type: {{"InventoryType"}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryheader"}}
        <td colspan="5" class="lower header-row">Category: {{Category}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "SubCategoryheader"}}
        <td colspan="5" class="lower header-row">Sub Category: {{SubCategory}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="text" data-value="{{ICode}}"></td>
        <td class="text" data-value="{{Description}}"></td>
        <td class="text" data-value="{{ChangeDate}}"></td>
        <td class="number" data-value="{{Quantity}}"></td>
        <td class="number" data-value="{{UnitValue}}"></td>
        <td class="number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryfooter"}}
        <td colspan="3" style="font-weight:bold;text-align:right;padding-top:11px">{{Category}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "SubCategoryfooter"}}
        <td colspan="3" style="font-weight:bold;text-align:right;padding-top:11px">{{SubCategory}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypefooter"}}
        <td colspan="3" style="font-weight:bold;text-align:right;padding-top:11px">{{InventoryType}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Warehousefooter"}}
        <td colspan="3" style="font-weight:bold;text-align:right;padding-top:11px">{{Warehouse}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "grandtotal"}}
        <td colspan="3" style="font-weight:bold;text-align:right;padding-top:11px">Grand Total:&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{/ifEquals}}

  <!--DETAIL VIEW TABLE-->
  {{#ifEquals ViewSetting "DetailView"}}
  <table>
    <thead>
      <tr>
        <th class="text">I-Code</th>
        <th class="text">Description</th>
        <th class="text">Date</th>
        <th class="text">Type</th>
        <th class="text">Change Description</th>
        <th class="number">Quantity</th>
        <th class="number">Unit Value</th>
        <th class="number">Extended</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr class="detail-view-row" data-rowtype={{RowType}}>
        {{#ifEquals RowType "Warehouseheader"}}
        <td colspan="5" class="header-row">Warehouse: {{Warehouse}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypeheader"}}
        <td colspan="5" class="lower header-row">Inventory Type: {{"InventoryType"}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryheader"}}
        <td colspan="5" class="lower header-row">Category: {{Category}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "SubCategoryheader"}}
        <td colspan="5" class="lower header-row">Sub Category: {{SubCategory}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="text" data-value="{{ICode}}"></td>
        <td class="text" data-value="{{Description}}"></td>
        <td class="text" data-value="{{ChangeDate}}"></td>
        <td class="text" data-value="{{ChangeType}}"></td>
        <td class="text" data-value="{{ChangeDescription}}"></td>
        <td class="number" data-value="{{Quantity}}"></td>
        <td class="number" data-value="{{UnitValue}}"></td>
        <td class="number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "ICodefooter"}}
        <td colspan="5" style="font-weight:bold;text-align:right;padding-top:11px">{{ICode}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "SubCategoryfooter"}}
        <td colspan="5" style="font-weight:bold;text-align:right;padding-top:11px">{{SubCategory}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Categoryfooter"}}
        <td colspan="5" style="font-weight:bold;text-align:right;padding-top:11px">{{Category}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "InventoryTypefooter"}}
        <td colspan="5" style="font-weight:bold;text-align:right;padding-top:11px">{{InventoryType}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Warehousefooter"}}
        <td colspan="5" style="font-weight:bold;text-align:right;padding-top:11px">{{Warehouse}}&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "grandtotal"}}
        <td colspan="5" style="font-weight:bold;text-align:right;padding-top:11px">Grand Total:&nbsp;</td>
        <td class="total-row number" data-value="{{Quantity}}"></td>
        <td colspan="2" class="total-row number" data-value="{{ExtendedValue}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{/ifEquals}}
</div>