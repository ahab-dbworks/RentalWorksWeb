<!--Return on Asset Precalculated-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">Invoice From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    <thead>
      <tr>
        <th class="text">Warehouse</th>
        <th class="text">Inventory Type</th>
        <th class="text">Category</th>
        <th class="text">Sub Category</th>
        <th class="text">ICode Rank</th>
        <th class="text">ICode</th>
        <th class="text">Description</th>
        <th class="number">Owned Qty</th>
        <th class="number">Days Owned</th>
        <th class="number">Avg Owned Qty</th>
        <th class="number">Avg Weekly Amt</th>
        <th class="number">Utilization (Billed)</th>
        <th class="number">Utilization (Out)</th>
        <th class="number">Annual ROA</th>
        <th class="number">Rental ROR</th>
        <th class="number">Annual ROR</th>
        <th class="number">Annual ROUV</th>
        <th class="number">Weeks Billed</th>
        <th class="number">Days Rented</th>
        <th class="number">Owned Revenue Amt</th>
        <th class="number">Vendor Revenue Amt</th>
        <th class="number">Average Cost</th>
        <th class="number">Asset Average Cost</th>
        <th class="number">Replacement Cost</th>
        <th class="number">Total Replacement Cost</th>
        <th class="number">Unit Value</th>
        <th class="number">Total Unit Value</th>
        <th class="number">Percent In Repair</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "Agentheader"}}
        <td colspan="3" class="name-row">Agent: {{Agent}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="text" data-value="{{InvoiceNumber}}"></td>
        <td class="text" data-value="{{InvoiceDate}}"></td>
        <td class="text" data-value="{{BillingStartDate}}"></td>
        <td class="text" data-value="{{InvoiceDescription}}"></td>
        <td class="text" data-value="{{OrderNumber}}"></td>
        <td class="text" data-value="{{Deal}}"></td>
        <td class="number" data-value="{{RentalTotal}}"></td>
        <td class="number" data-value="{{SalesTotal}}"></td>
        <td class="number" data-value="{{FacilitiesTotal}}"></td>
        <td class="number" data-value="{{LaborTotal}}"></td>
        <td class="number" data-value="{{MiscellaneousTotal}}"></td>
        <td class="number" data-value="{{AssetTotal}}"></td>
        <td class="number" data-value="{{PartsTotal}}"></td>
        <td class="number" data-value="{{InvoiceTax}}"></td>
        <td class="number" data-value="{{InvoiceTotal}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "Agentfooter"}}
        <td colspan="6" style="font-weight:bold;text-align:right;padding-top:11px">{{Agent}}&nbsp;</td>
        <td class="total-row number" data-value="{{RentalTotal}}"></td>
        <td class="total-row number" data-value="{{SalesTotal}}"></td>
        <td class="total-row number" data-value="{{FacilitiesTotal}}"></td>
        <td class="total-row number" data-value="{{LaborTotal}}"></td>
        <td class="total-row number" data-value="{{MiscellaneousTotal}}"></td>
        <td class="total-row number" data-value="{{AssetTotal}}"></td>
        <td class="total-row number" data-value="{{PartsTotal}}"></td>
        <td class="total-row number" data-value="{{InvoiceTax}}"></td>
        <td class="total-row number" data-value="{{InvoiceTotal}}"></td>
        {{/ifEquals}}
        {{#ifEquals RowType "grandtotal"}}
        <td colspan="6" style="font-weight:bold;text-align:right;padding-top:11px">Grand Total:&nbsp;</td>
        <td class="total-row number" data-value="{{RentalTotal}}"></td>
        <td class="total-row number" data-value="{{SalesTotal}}"></td>
        <td class="total-row number" data-value="{{FacilitiesTotal}}"></td>
        <td class="total-row number" data-value="{{LaborTotal}}"></td>
        <td class="total-row number" data-value="{{MiscellaneousTotal}}"></td>
        <td class="total-row number" data-value="{{AssetTotal}}"></td>
        <td class="total-row number" data-value="{{PartsTotal}}"></td>
        <td class="total-row number" data-value="{{InvoiceTax}}"></td>
        <td class="total-row number" data-value="{{InvoiceTotal}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>