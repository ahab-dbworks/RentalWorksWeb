<!--Late Returns Report-->
<div class="rpt-flexrow" style="width: 1600px;">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">{{subtitle}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;width: 1600px;">
  <table>
    <thead>
      <tr>
        <th class="text">Order No.</th>
        <th class="text">Date</th>
        <th class="text">Description</th>
        <th class="text">Agent</th>
        <th class="text">Ordered By</th>
        <th class="text">Billing Dates</th>
        {{#ifEquals ShowUnit true}}
        <th class="number">Unit Value</th>
        {{else}}
        <th></th>
        {{/ifEquals}}
        {{#ifEquals ShowReplacement true}}
        <th class="number">Replacement Cost</th>
        {{else}}
        <th></th>
        {{/ifEquals}}
        <th class="text">Rent From</th>
        <th class="text">Rent To</th>
        {{#ifEquals Type "PASTDUE"}}
        <th class="text">Days Past</th>
        {{else}}
        <th class="text">Due In</th>
        {{/ifEquals}}
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr data-rowtype="{{RowType}}">
        {{#ifEquals RowType "OfficeLocationheader"}}
        <td colspan="4" class="header-row">Office: {{OfficeLocation}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "Dealheader"}}
        <td colspan="4" class="lower header-row">Deal: {{Deal}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "OrderNumberheader"}}
        <td class="header-row text">{{OrderNumber}}</td>
        <td class="header-row text">{{OrderDate}}</td>
        <td class="header-row text">{{OrderDescription}}</td>
        <td class="header-row text">{{Agent}}</td>
        <td class="header-row text">{{OrderedByName}}</td>
        <td class="header-row text">{{BillDateRange}}</td>
        <td class="header-row number">{{OrderReplacementCost}}</td>
        <td class="header-row number">{{OrderFromDate}}</td>
        <td class="header-row number">{{OrderToDate}}</td>
        <td class="header-row number">{{OrderPastDue}}</td>
      </tr>
      <tr>
        <td class="order-header text">I-Code</td>
        <td class="order-header text">Description</td>
        {{#ifEquals ../ShowBarCode true}}
        <td class="order-header text">Bar Code</td>
        {{else}}
        <td class="order-header"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowSerial true}}
        <td class="order-header text">Serial No.</td>
        {{else}}
        <td class="order-header"></td>
        {{/ifEquals}}
        <td class="order-header number">Qty</td>
        {{#ifEquals ../ShowUnit true}}
        <td class="order-header number">Unit Value</td>
        {{else}}
        <td class="order-header"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="order-header number">Replacement Cost</td>
        {{else}}
        <td class="order-header"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowUnit true}}
        <td class="order-header  number">Unit Value Ext</td>
        {{else}}
        <td class="order-header"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="order-header number">Replacement Cost Ext</td>
        {{else}}
        <td class="order-header"></td>
        {{/ifEquals}}
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td class="text">{{ICode}}</td>
        <td class="text">{{Description}}</td>
        {{#ifEquals ../ShowBarCode true}}
        <td class="text">{{BarCode}}</td>
        {{else}}
        <td></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowSerial true}}
        <td class="text">{{SerialNumber}}</td>
        {{else}}
        <td></td>
        {{/ifEquals}}
        <td class="number">{{Quantity}}</td>
        {{#ifEquals ../ShowUnit true}}
        <td class="number">{{ItemUnitValue}}</td>
        {{else}}
        <td></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="number">{{ItemReplacementCost}}</td>
        {{else}}
        <td></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowUnit true}}
        <td class="number">{{ItemUnitValueExtended}}</td>
        {{else}}
        <td></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="number">{{ItemReplacementCostExtended}}</td>
        {{else}}
        <td></td>
        {{/ifEquals}}
        <td class="text">{{ItemFromDate}}</td>
        <td class="text">{{ItemToDate}}</td>
        <td class="number">{{ItemPastDue}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "OrderNumberfooter"}}
        <td colspan="4" class="total-name">{{OrderNumber}}&nbsp;</td>
        <td class="total-val" data-value="{{Quantity}}"></td>
        <td class="total-val" colspan="2"></td>
        {{#ifEquals ../ShowUnit true}}
        <td class="total-val" data-value="{{ItemUnitValueExtended}}"></td>
        {{else}}
        <td class="total-val"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="total-val" data-value="{{ItemReplacementCostExtended}}"></td>
        {{else}}
        <td class="total-val"></td>
        {{/ifEquals}}
        {{/ifEquals}}
        {{#ifEquals RowType "Dealfooter"}}
        <td colspan="4" class="total-name">{{Deal}}&nbsp;</td>
        <td class="total-val" data-value="{{Quantity}}"></td>
        <td class="total-val" colspan="2"></td>
        {{#ifEquals ../ShowUnit true}}
        <td class="total-val" data-value="{{ItemUnitValueExtended}}"></td>
        {{else}}
        <td class="total-val"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="total-val" data-value="{{ItemReplacementCostExtended}}"></td>
        {{else}}
        <td class="total-val"></td>
        {{/ifEquals}}
        {{/ifEquals}}
        {{#ifEquals RowType "OfficeLocationfooter"}}
        <td colspan="4" class="total-name">{{OfficeLocation}}&nbsp;</td>
        <td class="total-val" data-value="{{Quantity}}"></td>
        <td class="total-val" colspan="2"></td>
        {{#ifEquals ../ShowUnit true}}
        <td class="total-val" data-value="{{ItemUnitValueExtended}}"></td>
        {{else}}
        <td class="total-val"></td>
        {{/ifEquals}}
        {{#ifEquals ../ShowReplacement true}}
        <td class="total-val" data-value="{{ItemReplacementCostExtended}}"></td>
        {{else}}
        <td class="total-val"></td>
        {{/ifEquals}}
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
