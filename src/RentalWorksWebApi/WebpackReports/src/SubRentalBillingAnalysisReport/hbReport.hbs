<!--Sub-Rental Billing Analysis Report-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">Retired From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table style="border-top:2px solid black;">
    <!--<thead>
    <tr>
      <th>Order No.</th>
      <th>Date</th>
      <th>Description</th>
      <th>PO No.</th>
      <th>Date</th>
      <th>PO Classification</th>
      <th>Vendor</th>
      <th>Received Date</th>
      <th>Returned Date</th>
    </tr>
  </thead>-->
    {{#each this}}
    {{#ifEquals RowType "OfficeLocationheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th class="header-row noBorder" colspan="9">Office Location: {{OfficeLocation}}</th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "Departmentheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th class="header-row noBorder" colspan="9">Department: {{Department}}</th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "Dealheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th class="header-row noBorder" colspan="9">Deal: {{Deal}}</th>
      </tr>
    </thead>
    {{/ifEquals}}
    {{#ifEquals RowType "PurchaseOrderNumberheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th>Order No.</th>
        <th>Date</th>
        <th>Description</th>
        <th>PO No.</th>
        <th>Date</th>
        <th>PO Classification</th>
        <th>Vendor</th>
        <th>Received Date</th>
        <th>Returned Date</th>
      </tr>
    </thead>
    <tr>
      <td data-value="{{OrderNumber}}"></td>
      <td data-value="{{OrderDate}}"></td>
      <td data-value="{{OrderDescription}}"></td>
      <td data-value="{{PurchaseOrderNumber}}"></td>
      <td data-value="{{PoClassification}}"></td>
      <td data-value="{{Vendor}}"></td>
      <td data-value="{{OrderNumber}}"></td>
      <td data-value="{{ReceiveDate}}"></td>
      <td data-value="{{ReturnDate}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "InvoiceNumberheader"}}
    <thead>
      <tr data-rowtype={{RowType}}>
        <th class="noBorder"></th>
        <th>Invoice No.</th>
        <th>Date</th>
        <th colspan="2">Usage Period</th>
        <th>Vendor Invoice No.</th>
        <th>Date</th>
        <th colspan="2">Usage Period</th>
      </tr>
    </thead>
    <tr>
      <td class="noBorder"></td>
      <td data-value="{{InvoiceNumber}}"></td>
      <td data-value="{{InvoiceDate}}"></td>
      <td data-value="{{InvoiceBillingStart}}"></td>
      <td data-value="{{InvoiceBillingEnd}}"></td>
      <td data-value="{{VendorInvoiceNumber}}"></td>
      <td data-value="{{VendorInvoiceDate}}"></td>
      <td data-value="{{VendorBillingStart}}"></td>
      <td data-value="{{VendorBillingEnd}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "detail"}}
    <thead>
      <tr>
        <th class="noBorder"></th>
        <th>I-Code</th>
        <th>Description</th>
        <th class="number">Qty</th>
        <th>Usage Period</th>
        <th>Vendor Usage Period</th>
        <th class="number">Deal Billed</th>
        <th class="number">Vendor Amount</th>
        <th class="number">Variance</th>
      </tr>
    </thead>
    <tbody data-rowtype="{{RowType}}">
      <tr>
        <th class="noBorder"></th>
        <td data-value="{{ICode}}"></td>
        <td data-value="{{Description}}"></td>
        <td data-value="{{Quantity}}"></td>
        <td data-value="{{DealItemDateRange}}"></td>
        <td data-value="{{VendorItemDateRange}}"></td>
        <td class="number" data-value="{{ItemBilled}}"></td>
        <td class="number" data-value="{{ItemCost}}"></td>
        <td class="number" data-value="{{ItemVariance}}"></td>
      </tr>
    </tbody>
    {{/ifEquals}}
  <tfoot>
    {{#ifEquals RowType "InvoiceNumberfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="6" class="total-name">{{InvoiceNumber}}&nbsp;</td>
      <td class="total-val" data-value="{{ItemBilled}}"></td>
      <td class="total-val" data-value="{{ItemCost}}"></td>
      <td class="total-val" data-value="{{ItemVariance}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "PurchaseOrderNumberfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="6" class="total-name">{{PurchaseOrderNumber}}&nbsp;</td>
      <td class="total-val" data-value="{{ItemBilled}}"></td>
      <td class="total-val" data-value="{{ItemCost}}"></td>
      <td class="total-val" data-value="{{ItemVariance}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "Dealfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="6" class="total-name">{{Deal}}&nbsp;</td>
      <td class="total-val" data-value="{{ItemBilled}}"></td>
      <td class="total-val" data-value="{{ItemCost}}"></td>
      <td class="total-val" data-value="{{ItemVariance}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "Departmentfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="6" class="total-name">{{Department}}&nbsp;</td>
      <td class="total-val" data-value="{{ItemBilled}}"></td>
      <td class="total-val" data-value="{{ItemCost}}"></td>
      <td class="total-val" data-value="{{ItemVariance}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "OfficeLocationfooter"}}
    <tr data-rowtype="{{RowType}}">
      <td colspan="6" class="total-name">{{OfficeLocation}}&nbsp;</td>
      <td class="total-val" data-value="{{ItemBilled}}"></td>
      <td class="total-val" data-value="{{ItemCost}}"></td>
      <td class="total-val" data-value="{{ItemVariance}}"></td>
    </tr>
    {{/ifEquals}}
    {{#ifEquals RowType "grandtotal"}}
  <tr data-rowtype="{{RowType}}">
    <td colspan="6" class="total-name">Grand Total:&nbsp;</td>
    <td class="total-val" data-value="{{ItemBilled}}"></td>
    <td class="total-val" data-value="{{ItemCost}}"></td>
    <td class="total-val" data-value="{{ItemVariance}}"></td>
  </tr>
    {{/ifEquals}}
    {{/each}}
  </tfoot>
  </table>
</div>