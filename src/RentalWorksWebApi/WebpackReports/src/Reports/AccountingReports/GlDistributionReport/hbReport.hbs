<!--G/L Distribution Report-->
<div data-section="header" class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<!--SUMMARY VIEW TABLE-->
{{#ifEquals IsSummary true}}
<div data-section="table" class="rpt-flexrow" style="margin-top:40px;">
  <table data-tablename="Summary View">
    <thead id="columnHeader">
      <tr data-row="main-header">
        <th data-linkedcolumn="AccountNumber" data-valuefield="AccountNumber">G/L No.</th>
        <th data-linkedcolumn="AccountDescription" data-valuefield="AccountDescription">Account Description</th>
        <th data-linkedcolumn="Debit" data-valuefield="Debit" class="number">Debit</th>
        <th data-linkedcolumn="Credit" data-valuefield="Credit" class="number">Credit</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      {{#ifEquals RowType "Locationheader"}}
      <tr data-row="header" data-rowtype="{{RowType}}">
        <td colspan="5" class="header-row">Location: {{Location}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "GroupHeadingheader"}}
      <tr data-row="header" data-rowtype="{{RowType}}">
        <td colspan="4" class="header-row">{{GroupHeading}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "detail"}}
      <tr data-row="detail" data-rowtype="{{RowType}}">
        <td data-linkedcolumn="AccountNumber" data-value="{{AccountNumber}}"></td>
        <td data-linkedcolumn="AccountDescription" data-value="{{AccountDescription}}"></td>
        <td data-linkedcolumn="Debit" class="number" data-value="{{Debit}}"></td>
        <td data-linkedcolumn="Credit" class="number" data-value="{{Credit}}"></td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "GroupHeadingfooter"}}
      <tr data-row="footer" data-rowtype="{{RowType}}">
        <td colspan="2" class="total-name">{{GroupHeading}}&nbsp;</td>
        <td data-linkedcolumn="Debit" class="total-val" data-value="{{Debit}}"></td>
        <td data-linkedcolumn="Credit" class="total-val" data-value="{{Credit}}"></td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "Locationfooter"}}
      <tr data-row="footer" data-rowtype="{{RowType}}">
        <td colspan="2" class="total-name">{{Location}}&nbsp;</td>
        <td data-linkedcolumn="Debit" class="total-val" data-value="{{Debit}}"></td>
        <td data-linkedcolumn="Credit" class="total-val" data-value="{{Credit}}"></td>
      </tr>
      {{/ifEquals}}
      {{/each}}
    </tbody>
  </table>
  {{/ifEquals}}
</div>
