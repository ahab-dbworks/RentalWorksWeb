<!--Order Report-->
<div data-section="header">
  <div class="rpt-flexrow" style="margin:5px 0px 0px 4px;">
    <div class="rpt-flexcolumn" style="flex:0 0 30%;padding-bottom:5px;">
      <div class="rpt-nested-flexrow">
        <img data-visible="{{#if Logosrc}}true{{/if}}" class="clientlogo" src="{{Logosrc}}" style="display:block;width:auto;height:100px;" alt="logo" />
      </div>
      <div class="rpt-nested-flexrow header-left"><span style="font-weight:600">{{OfficeLocationCompany}}</span></div>
      <div class="rpt-nested-flexrow header-left"><span>{{OfficeLocationAddress1}}</span></div>
      <div data-visible="{{#if OfficeLocationAddress2}}true{{/if}}" class="rpt-nested-flexrow header-left"><span>{{OfficeLocationAddress2}}</span></div>
      <div class="rpt-nested-flexrow header-left"><span>{{OfficeLocationCityStateZipCodeCountry}}</span></div>
      <div class="rpt-nested-flexrow header-left"><span style="font-weight:600;">Phone: </span><span>{{OfficeLocationPhone}}</span></div>
    </div>
    <div class="rpt-flexcolumn" style="flex:0 0 40%;">
      <div class="rpt-nested-flexrow sub-title"><span style="font-size:20px;">{{Department}}</span></div>
      <div class="rpt-nested-flexrow title"><span>{{Report}}</span></div>
    </div>
    <!-- Order Number & Date -->
    <div class="rpt-flexcolumn" style="flex:0 0 30%;">
      <div class="rpt-nested-flexrow">
        <span style="width:30%;font-size:18px;font-weight:700;text-align:right;vertical-align:middle;">No:</span>
        <span style="background-color: #e3f2fd; color: #027cc5; border: 2px solid #212121; padding: 5px 10px; font-size: 26px; text-align: center;width: 70%;">{{OrderNumber}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="width: 30%; color: black; font-size: 18px; font-weight: 700; text-align: right;">Date:</span>
        <span style="width: 70%; font-size: 18px; text-align: center;">{{PrintDate}}</span>
      </div>
    </div>
  </div>
  <!-- Order / Agent / Dates, Terms, Billing Cycle -->
  <div class="rpt-flexrow">
    <!-- Order details -->
    <div class="rpt-flexcolumn sectionBorder" style="flex:0 0 422px;margin-right:10px;">
      <div class="rpt-nested-flexrow">
        <span style="min-width:90px;" class="shaded-block">Description:</span>
        <span>{{Description}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width: 90px;" class="shaded-block">Customer:</span>
        <span>{{Customer}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width:90px;" class="shaded-block">Deal:</span>
        <span>{{Deal}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width:90px;" class="shaded-block">PO No:</span>
        <span>{{PoNumber}}</span>
      </div>
      <div data-visible="{{#if Location}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:90px;" class="shaded-block">Location:</span>
        <span>{{Location}}</span>
      </div>
    </div>
    <!-- Agent details -->
    <div class="rpt-flexcolumn sectionBorder" style="flex:0 0 321px;margin:0px 10px;">
      <div class="rpt-nested-flexrow">
        <span style="min-width:70px;" class="shaded-block">Agent:</span>
        <span>{{Agent}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width:70px;" class="shaded-block">Email:</span>
        <span>{{AgentEmail}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width:70px;" class="shaded-block">Phone:</span>
        <span>{{AgentPhone}}</span>
      </div>
    </div>
    <!-- Billing Dates, Usage Dates, Terms, Billing Cycle-->
    <div class="rpt-flexcolumn sectionBorder" style="flex:0 0 321px;margin-left:10px;">
      <div data-visible="{{#if HasRecurring}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Billing Period:</span>
        <span>{{BillingStartDate}}</span> - <span>{{BillingStopDate}}</span>
      </div>
      <div data-visible="{{#if HasRecurring}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Usage Period:</span>
        <span>{{EstimatedStartDate}}</span> - <span>{{EstimatedStopDate}}</span>
      </div>
      <div data-visible="{{#ifNotEquals HasRecurring true}}true{{/ifNotEquals}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Start Date:</span>
        <span>{{EstimatedStartDate}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Terms:</span>
        <span>{{PaymentTerms}}</span>
      </div>
      <div class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Billing Cycle:</span>
        <span>{{BillingCycle}}</span>
      </div>
      <div data-visible="{{#if ReferenceNumber}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Reference No:</span>
        <span>{{ReferenceNumber}}</span>
      </div>
    </div>
  </div>
  <!-- Issued To / Outgoing /Incoming -->
  <div class="rpt-flexrow" style="margin-top:25px;">
    <div class="rpt-flexcolumn" style="flex:0 0 422px;margin-right:10px;">
      <div class="rpt-nested-flexrow"><span class="sub text" style="min-width:400px;text-align:left;padding:5px;border-top: 2px solid #027cc5;border-bottom: 2px solid #027cc5;background-color: #e3f2fd;">Issued To</span></div>
      <div class="rpt-nested-flexrow"><span style="font-weight:700;">{{IssuedToCompany}}</span></div>
      <div data-visible="{{#if IssuedToAttention1}}true{{/if}}" class="rpt-nested-flexrow"><span>{{IssuedToAttention1}}</span></div>
      <div data-visible="{{#if IssuedToAttention2}}true{{/if}}" class="rpt-nested-flexrow"><span>{{IssuedToAttention2}}</span></div>
      <div class="rpt-nested-flexrow"><span>{{IssuedToAddress1}}</span></div>
      <div data-visible="{{#if IssuedToAddress2}}true{{/if}}" class="rpt-nested-flexrow"><span>{{IssuedToAddress2}}</span></div>
      <div class="rpt-nested-flexrow"><span>{{IssuedToCity}}</span>,&nbsp;<span>{{IssuedToState}}</span>&nbsp;<span>{{IssuedToZipCode}}</span>&nbsp;<span>{{IssuedToCountry}}</span></div>
      <div data-visible="{{#if OrderedBy}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Ordered By:</span>
        <span>{{OrderedBy}}</span>
      </div>
      <div data-visible="{{#if OrderedByEmail}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Email:</span>
        <span>{{OrderedByEmail}}</span>
      </div>
      <div data-visible="{{#if OrderedByPhoneAndExtension}}true{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;" class="shaded-block">Phone:</span>
        <span>{{OrderedByPhoneAndExtension}}</span>
      </div>
    </div>
    <div class="rpt-flexcolumn" style="flex:0 0 321px;margin:0px 10px;">
      <div class="rpt-nested-flexrow"><span class="sub text" style="min-width:300px;text-align:left;padding:5px;border-top: 2px solid #027cc5;border-bottom: 2px solid #027cc5;background-color: #e3f2fd;">Outgoing ({{OutDeliveryType}})</span></div>
      <div class="rpt-nested-flexrow"><span>{{OutDeliveryLocation}}</span></div>
      <div data-visible="{{#if OutDeliveryAttention}}true{{/if}}" class="rpt-nested-flexrow"><span style="font-weight:700;">Attention: </span><span>{{OutDeliveryAttention}}</span></div>
      <div class="rpt-nested-flexrow"><span>{{OutDeliveryAddress1}}</span></div>
      <div data-visible="{{#if OutDeliveryAddress2}}true{{/if}}" class="rpt-nested-flexrow"><span>{{OutDeliveryAddress2}}</span></div>
      <div class="rpt-nested-flexrow"><span>{{OutDeliveryCity}}</span>,&nbsp;<span>{{OutDeliveryState}}</span>&nbsp;<span>{{OutDeliveryZipCode}}</span>&nbsp;<span>{{OutDeliveryCountry}}</span></div>

      <div data-visible="{{#if OutDeliveryRequiredByDate}}{{#ifCond OutDeliveryRequiredByTime '===' "  :"}}true{{/ifCond}}{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;font-weight:700;" class="shaded-block">Required By: </span>
        <span>{{OutDeliveryRequiredByDate}}</span>
      </div>
      <div data-visible="{{#if OutDeliveryRequiredByDate}}{{#ifCond OutDeliveryRequiredByTime '!==' "  :"}}true{{/ifCond}}{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;font-weight:700;" class="shaded-block">Required By: </span>
        <span>{{OutDeliveryRequiredByDate}}</span>&nbsp;<span>{{OutDeliveryRequiredByTime}}</span>
      </div>
    </div>
    <div data-visible="{{#ifCond RentalItems.length '>' 0}}true{{/ifCond}}" class="rpt-flexcolumn" style="flex:0 0 321px;margin:0px 10px;">
      <div class="rpt-nested-flexrow"><span class="sub text" style="min-width:300px;text-align:left;padding:5px;border-top: 2px solid #027cc5;border-bottom: 2px solid #027cc5;background-color: #e3f2fd;">Incoming ({{InDeliveryType}})</span></div>
      <div class="rpt-nested-flexrow"><span>{{InDeliveryLocation}}</span></div>
      <div data-visible="{{#if InDeliveryAttention}}true{{/if}}" class="rpt-nested-flexrow"><span style="font-weight:700;">Attention: </span><span>{{InDeliveryAttention}}</span></div>
      <div class="rpt-nested-flexrow"><span>{{InDeliveryAddress1}}</span></div>
      <div data-visible="{{#if InDeliveryAddress2}}true{{/if}}" class="rpt-nested-flexrow"><span>{{InDeliveryAddress2}}</span></div>
      <div class="rpt-nested-flexrow"><span>{{InDeliveryCity}}</span>,&nbsp;<span>{{InDeliveryState}}</span>&nbsp;<span>{{InDeliveryZipCode}}</span>&nbsp;<span>{{InDeliveryCountry}}</span></div>
      <div data-visible="{{#if InDeliveryRequiredByDate}}{{#ifCond InDeliveryRequiredByTime '===' "  :"}}true{{/ifCond}}{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;font-weight:700;" class="shaded-block">Required By: </span>
        <span>{{InDeliveryRequiredByDate}}</span>
      </div>
      <div data-visible="{{#if InDeliveryRequiredByDate}}{{#ifCond InDeliveryRequiredByTime '!==' "  :"}}true{{/ifCond}}{{/if}}" class="rpt-nested-flexrow">
        <span style="min-width:105px;font-weight:700;" class="shaded-block">Required By: </span>
        <span>{{InDeliveryRequiredByDate}}</span>&nbsp;<span>{{InDeliveryRequiredByTime}}</span>
      </div>
    </div>
  </div>
</div>
<!-- Rental Items -->
<div data-section="table" data-itemarray="RentalItems" class="rpt-flexrow">
  <table data-tablename="Rental Items" style="table-layout:fixed;">
    {{#each RentalItems}}
    <thead id="columnHeader">
      <tr data-row="main-header" data-linkedrow="detail">
        <th data-linkedcolumn="ICode" data-valuefield="ICode"></th>
        <th data-linkedcolumn="Description" data-valuefield="Description"></th>
        <th data-linkedcolumn="QuantityOrdered" data-valuefield="QuantityOrdered"></th>
        <th data-linkedcolumn="BillablePeriods" data-valuefield="BillablePeriods"></th>
        <th data-linkedcolumn="Rate" data-valuefield="Rate"></th>
        <th data-linkedcolumn="DiscountPercent" data-valuefield="DiscountPercent"></th>
        <th data-linkedcolumn="PeriodExtended" data-valuefield="PeriodExtended"></th>
      </tr>
    </thead>
    <tbody data-rowtype="{{RowType}}">
      {{#ifEquals RowType "RecTypeDisplayheader"}}
      <tr data-row="header"><td colspan="7">&nbsp;</td></tr><!--spacer row-->
      <tr data-row="header" class="avoid-page-break">
        <td colspan="7" style="border: 1px dashed #027cc5;color: #027cc5;font-weight: bold;text-align: left;font-size: 35px;padding-top: 5px;">{{RecTypeDisplay}}&nbsp;</td>
      </tr>
      <tr data-row="linked-sub-header" data-sort="true" data-linkedrow="detail" class="rectype-header">
        <th data-linkedcolumn="ICode" data-valuefield="ICode" style="min-width:90px;">I-Code</th>
        <th data-linkedcolumn="Description" data-valuefield="Description" style="min-width:504px;">Description</th>
        <th data-linkedcolumn="QuantityOrdered" data-valuefield="QuantityOrdered" style="min-width:65px;" class="number">Qty.</th>
        <th data-linkedcolumn="BillablePeriods" data-valuefield="BillablePeriods" style="min-width:75px;" class="number">{{BillablePeriodUnits}}</th>
        <th data-linkedcolumn="Rate" data-valuefield="Rate" style="min-width:100px;" class="number">Price</th>
        <th data-linkedcolumn="DiscountPercent" data-valuefield="DiscountPercent" data-visible="{{#if HasDiscount}}true{{/if}}" style="min-width:65px;" class="number">Disc. %</th>
        <th data-linkedcolumn="PeriodExtended" data-valuefield="PeriodExtended" style="min-width:140px;" class="number">Period Extended</th>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "detail"}}
      <tr data-row="detail" data-rowtype="{{RowType}}" data-isbold="{{Bold}}">
        <td data-linkedcolumn="ICode" data-value="{{ICode}}"></td>
        <td data-linkedcolumn="Description" data-value="{{Description}}"></td>
        <td data-linkedcolumn="QuantityOrdered" class="number" data-value="{{QuantityOrdered}}"></td>
        <td data-linkedcolumn="BillablePeriods" class="number" data-value="{{BillablePeriods}}"></td>
        <td data-linkedcolumn="Rate" class="number" data-value="{{Rate}}"></td>
        <td data-linkedcolumn="DiscountPercent" class="number" data-value="{{DiscountPercent}}"></td>
        <td data-linkedcolumn="PeriodExtended" class="number" data-value="{{PeriodExtended}}"></td>
      </tr>
      {{#if Notes}}
      <tr data-row="sub-detail" data-sort="true">
        <td class="alignRight"><div class="note-label">Note:&nbsp;</div></td>
        <td colspan="5"><div class="note-cell">{{Notes}}</div></td>
        <td>&nbsp;</td>
      </tr>
      {{/if}}
      {{/ifEquals}}
      {{#ifEquals RowType "RecTypeDisplayfooter"}}
      {{#ifNotEquals PeriodTaxNoCurrency 0}}
      <tr data-row="footer">
        <td colspan="5" class="total-name"></td>
        <td data-linkedcolumn="DiscountPercent" style="text-align:right;font-weight:700;padding-top:12px;padding-right:0px;"><span style="padding:11px 5px 3px 6px;border-top:1px solid #000000;">{{RecTypeDisplay}} Sub-Total</span></td>
        <td data-linkedcolumn="PeriodExtended" style="text-align:right;font-weight:700;border-top: 1px solid black;padding-top:10px;" data-value="{{PeriodExtended}}"></td>
      </tr>
      <tr data-row="footer">
        <td colspan="5" class="total-name"></td>
        <td data-linkedcolumn="DiscountPercent" style="text-align: right;font-weight: 700;padding: 5px 3px 3px 0;">{{../Tax1Name}} ({{TaxRate1}}%)</td>
        <td data-linkedcolumn="PeriodExtended" style="text-align: right;font-weight: 700;padding: 5px 3px 3px 0;" data-value="{{PeriodTax1}}"></td>
      </tr>
      {{#ifCond ../Tax2Name '!=' ""}}
      <tr data-row="footer">
        <td colspan="5" class="total-name"></td>
        <td data-linkedcolumn="DiscountPercent" style="text-align: right;font-weight: 700;padding: 5px 3px 3px 0;">{{../Tax2Name}} ({{TaxRate2}}%)</td>
        <td data-linkedcolumn="PeriodExtended" style="text-align: right;font-weight: 700;padding: 5px 3px 3px 0;" data-value="{{PeriodTax2}}"></td>
      </tr>
      {{/ifCond}}
      {{/ifNotEquals}}
      <tr data-row="footer">
        <td colspan="5" class="total-name"></td>
        <td data-linkedcolumn="DiscountPercent" style="text-align:right;font-weight:700;padding-top:12px;padding-right:0px;"><span style="background-color: #e3f2fd;font-weight: 600;vertical-align: top;padding:11px 5px 3px 41px;border-top:1px solid #000000;">{{RecTypeDisplay}} Total</span></td>
        <td data-linkedcolumn="PeriodExtended" style="background-color: #f4ffcc;width: 110px;text-align: right;font-weight: bold;border-top: 1.5px solid black;padding-top: 10px;" data-value="{{PeriodExtendedWithTax}}"></td>
      </tr>
      {{/ifEquals}}
    </tbody>
    {{/each}}
  </table>
</div>
