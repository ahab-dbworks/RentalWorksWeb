<!--Order Report-->
<div class="rpt-flexrow" style="margin:5px 0px 0px 4px;">
  <div class="rpt-flexcolumn" style="flex:0 0 30%;padding-bottom:5px;">
    {{#if Logosrc}}
    <div style="height:100px;"><img class="clientlogo" src="{{Logosrc}}" style="display:block;width:auto;height:100%;" alt="logo" /></div>
    {{else}}
    <div></div>
    {{/if}}
    <span class="header-left" style="font-weight:600">{{OfficeLocationCompany}}</span>
    <span class="header-left">{{OfficeLocationAddress1}}</span>
    <span class="header-left">{{OfficeLocationAddress2}}</span>
    <span class="header-left">{{OfficeLocationCityStateZipCodeCountry}}</span>
    <span class="header-left"><span style="font-weight:600;">Phone: </span>{{OfficeLocationPhone}}</span>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 40%;">
    <span class="sub-title" style="font-size:20px;">{{Department}}</span>
    <span class="title blue">{{Report}}</span>
  </div>
  <!-- Order Number & Date -->
  <div class="rpt-flexcolumn" style="flex:0 0 30%;">
    <table>
      <tr>
        <td style="width:50%;font-size:18px;font-weight:700;text-align:right;">No:</td>
        <td style="width:50%;background-color:#e3f2fd;color:#027cc5;border:2px solid #212121;padding:5px 10px;font-size:26px;text-align:center;">{{OrderNumber}}</td>
      </tr>
      <tr>
        <td style="width:50%;color:black;font-size:18px;font-weight:700;text-align:right;">Date:</td>
        <td style="width:50%;font-size:18px;text-align:center;">{{PrintDate}}</td>
      </tr>
    </table>
  </div>
</div>
<!-- Order / Agent / Dates, Terms, Billing Cycle -->
<div class="rpt-flexrow">
  <!-- Order details -->
  <div class="rpt-flexcolumn" style="flex:0 0 422px;border-top: 2px solid #027cc5;margin-right:10px;">
    <table>
      <tr>
        <td style="width:90px;background-color:#e3f2fd;font-weight:700;">Description:</td>
        <td class="">{{Description}}</td>
      </tr>
      <tr>
        <td style="width:90px;background-color:#e3f2fd;font-weight:700;">Customer:</td>
        <td class="">{{Customer}}</td>
      </tr>
      <tr>
        <td style="width:90px;background-color:#e3f2fd;font-weight:700;">Deal:</td>
        <td class="">{{Deal}}</td>
      </tr>
      <tr>
        <td style="width:90px;background-color:#e3f2fd;font-weight:700;">PO No:</td>
        <td class="">{{PoNumber}}</td>
      </tr>
    </table>
  </div>
  <!-- Agent details -->
  <div class="rpt-flexcolumn" style="flex:0 0 321px;border-top: 2px solid #027cc5;margin:0px 10px;">
    <table>
      <tr>
        <td style="width:55px;background-color:#e3f2fd;font-weight:700;">Agent:</td>
        <td class="">{{Agent}}</td>
      </tr>
      <tr>
        <td style="width:55px;background-color:#e3f2fd;font-weight:700;">Email:</td>
        <td class="">{{AgentEmail}}</td>
      </tr>
      <tr>
        <td style="width:55px;background-color:#e3f2fd;font-weight:700;">Phone:</td>
        <td class="">{{AgentPhone}}</td>
      </tr>
    </table>
  </div>
  <!-- Billing Dates, Usage Dates, Terms, Billing Cycle-->
  <div class="rpt-flexcolumn" style="flex:0 0 321px;border-top: 2px solid #027cc5;margin-left:10px;">
    <table>
      {{#ifCond BillingDates '!==' "N/A"}}
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Billing Period:</td>
        <td class="">{{BillingDates}}</td>
      </tr>
      {{/ifCond}}
      {{#ifCond UsageDates '!==' "N/A"}}
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Usage Period:</td>
        <td class="">{{UsageDates}}</td>
      </tr>
      {{/ifCond}}
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Terms:</td>
        <td class="">{{PaymentTerms}}</td>
      </tr>
      <tr>
        <td style="width:105px;background-color:#e3f2fd;;font-weight:700;">Billing Cycle:</td>
        <td class="">{{BillingCycle}}</td>
      </tr>
    </table>
  </div>
</div>
<!-- Issued To / Outgoing /Incoming -->
