<!--Rental Inventory Movement Report-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    <thead class="main-header">
      <tr>
        <th>I-Code</th>
        <th>Description</th>
        <th>Rank</th>
        <th class="number">From Date Owned</th>
        <th class="number">From Date Value</th>
        <th class="number">Purchased</th>
        <th class="number">Retired</th>
        <th class="number">Unretired</th>
        <th class="number">Transferred Out</th>
        <th class="number">Transferred In</th>
        <th class="number">To Date Owned</th>
        <th class="number">To Date Value</th>
        <th class="number">Last Purchase</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      {{#ifEquals RowType "Warehouseheader"}}
      <tr>
        <td colspan="5" class="lower header-row">Warehouse: {{"Warehouse"}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "InventoryTypeheader"}}
      <tr>
        <td colspan="5" class="lower header-row">Inventory Type: {{"InventoryType"}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "Categoryheader"}}
      <tr>
        <td colspan="5" class="lower header-row">Category: {{"Category"}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "detail"}}
    <tr data-rowtype="{{RowType}}">
      <td class="nowrap" data-value="{{ICode}}"></td>
      <td data-value="{{Description}}"></td>
      <td data-value="{{Rank}}"></td>
      <td class="number" data-value="{{OwnedFromDate}}"></td>
      <td class="number" data-value="{{ReplacementCostFromDate}}"></td>
      <td class="number" data-value="{{Purchased}}"></td>
      <td class="number" data-value="{{Retired}}"></td>
      <td class="number" data-value="{{Unretired}}"></td>
      <td class="number" data-value="{{TransferredOut}}"></td>
      <td class="number" data-value="{{TransferredIn}}"></td>
      <td class="number" data-value="{{OwnedToDate}}"></td>
      <td class="number" data-value="{{ReplacementCostToDate}}"></td>
      <td class="number" data-value="{{LastPurchase}}"></td>
    </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "Categoryfooter"}}
    <tr>
      <td colspan="3" class="total-name">{{Category}}:&nbsp;</td>
      <td class="total-val" data-value="{{OwnedFromDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostFromDate}}"></td>
      <td class="total-val" data-value="{{Purchased}}"></td>
      <td class="total-val" data-value="{{Retired}}"></td>
      <td class="total-val" data-value="{{Unretired}}"></td>
      <td class="total-val" data-value="{{TransferredOut}}"></td>
      <td class="total-val" data-value="{{TransferredIn}}"></td>
      <td class="total-val" data-value="{{OwnedToDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostToDate}}"></td>
      <td class="total-val" data-value="{{LastPurchase}}"></td>
    </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "InventoryTypefooter"}}
    <tr>
      <td colspan="3" class="total-name">{{InventoryType}}:&nbsp;</td>
      <td class="total-val" data-value="{{OwnedFromDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostFromDate}}"></td>
      <td class="total-val" data-value="{{Purchased}}"></td>
      <td class="total-val" data-value="{{Retired}}"></td>
      <td class="total-val" data-value="{{Unretired}}"></td>
      <td class="total-val" data-value="{{TransferredOut}}"></td>
      <td class="total-val" data-value="{{TransferredIn}}"></td>
      <td class="total-val" data-value="{{OwnedToDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostToDate}}"></td>
      <td class="total-val" data-value="{{LastPurchase}}"></td>
    </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "Warehousefooter"}}
    <tr>
      <td colspan="3" class="total-name">{{Warehouse}}:&nbsp;</td>
      <td class="total-val" data-value="{{OwnedFromDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostFromDate}}"></td>
      <td class="total-val" data-value="{{Purchased}}"></td>
      <td class="total-val" data-value="{{Retired}}"></td>
      <td class="total-val" data-value="{{Unretired}}"></td>
      <td class="total-val" data-value="{{TransferredOut}}"></td>
      <td class="total-val" data-value="{{TransferredIn}}"></td>
      <td class="total-val" data-value="{{OwnedToDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostToDate}}"></td>
      <td class="total-val" data-value="{{LastPurchase}}"></td>
    </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "grandtotal"}}
    <tr>
      <td colspan="3" class="total-name">Grand Total:&nbsp;</td>
      <td class="total-val" data-value="{{OwnedFromDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostFromDate}}"></td>
      <td class="total-val" data-value="{{Purchased}}"></td>
      <td class="total-val" data-value="{{Retired}}"></td>
      <td class="total-val" data-value="{{Unretired}}"></td>
      <td class="total-val" data-value="{{TransferredOut}}"></td>
      <td class="total-val" data-value="{{TransferredIn}}"></td>
      <td class="total-val" data-value="{{OwnedToDate}}"></td>
      <td class="total-val" data-value="{{ReplacementCostToDate}}"></td>
      <td class="total-val" data-value="{{LastPurchase}}"></td>
    </tr>
      {{/ifEquals}}
      {{/each}}
    </tbody>
  </table>
</div>