<!--Invoice Report-->
<div class="rpt-flexrow" style="margin:5px 0px 0px 4px;">
  <div class="rpt-flexcolumn" style="flex:0 0 30%;padding-bottom:5px;">
    {{#if Logosrc}}
    <div style="height:100px;">
      <img class="clientlogo" src="{{Logosrc}}" style="display:block;width:auto;height:100%;" alt="logo" />
    </div>
    {{else}}
    <div></div>
    {{/if}}
    <span style="font-weight:600; font-size:15px;">{{OfficeLocationCompany}}</span>
    <span class="company-header">{{OfficeLocationAddress1}}</span>
    {{#if OfficeLocationAddress2}}
    <span class="company-header">{{OfficeLocationAddress2}}</span>
    {{/if}}
    <span class="company-header">{{OfficeLocationCityStateZipCodeCountry}}</span>
    <span class="company-header"><span style="font-weight:600;">Phone: </span>{{OfficeLocationPhone}}</span>
    {{#if OfficeLocationFax}}
    <span class="company-header"><span style="font-weight:600;">Fax: </span>{{OfficeLocationFax}}</span>
    {{/if}}
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 40%;">
    <span class="sub-title" style="font-size:20px;">{{Department}}</span>
    <span class="title blue">{{Report}}</span>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 30%;">
    <table>
      <tr>
        <td style="width:50%;font-size:18px;font-weight:700;text-align:right;">No:</td>
        <td style="background-color:#e3f2fd;color:#027cc5;border:2px solid #212121;padding:5px 10px;font-size:26px;text-align:center;">{{InvoiceNumber}}</td>
      </tr>
      <tr>
        <td style="width:50%;color:black;font-size:18px;font-weight:700;text-align:right;">Invoice Date:</td>
        <td style="font-size:18px;text-align:center;">{{InvoiceDate}}</td>
      </tr>
      <tr>
        <td style="width:50%;color:black;font-size:18px;font-weight:700;text-align:right;">Due Date:</td>
        <td style="font-size:18px;text-align:center;">{{InvoiceDueDate}}</td>
      </tr>
    </table>


  </div>
</div>
<div class="rpt-flexrow" style="padding-left:3px;">
  <div class="rpt-flexcolumn" style="flex:0 0 422px;border-top: 2px solid #027cc5;padding:1px;margin-right:10px;">
    <table>
      <tr>
        <td style="width:80px;" class="shaded-block">Order: </td>
        <td class="meta-content" style="vertical-align:top;">{{OrderNumberAndDescription}}</td>
      </tr>
      <tr>
        <td style="width:80px;" class="shaded-block">Customer: </td>
        <td class="meta-content" style="vertical-align:top;">{{Customer}}</td>
      </tr>
      <tr>
        <td style="width:80px;" class="shaded-block">Deal: </td>
        <td class="meta-content" style="vertical-align:top;">{{DealAndDealNumber}}</td>
      </tr>
      <tr>
        <td style="width:80px;" class="shaded-block">PO No: </td>
        <td class="meta-content">{{PoNumber}}</td>
      </tr>
      {{#if Location}}
      <tr>
        <td class="shaded-block" style="width:90px;">Location: </td>
        <td class="meta-content">{{Location}}</td>
      </tr>
      {{/if}}
    </table>
  </div>
  <!-- Agent -->
  <div class="rpt-flexcolumn" style="flex:0 0 321px;border-top: 2px solid #027cc5;padding:1px;margin:0px 10px;">
    <table>
      <tr>
        <td style="width:55px;" class="shaded-block">Agent: </td>
        <td class="meta-content">{{Agent}}</td>
      </tr>
      <tr>
        <td style="width:55px;" class="shaded-block">Email: </td>
        <td class="meta-content">{{AgentEmail}}</td>
      </tr>
      <tr>
        <td style="width:55px;" class="shaded-block">Phone: </td>
        <td class="meta-content">{{AgentPhoneAndExtension}}</td>
      </tr>
    </table>
  </div>
  <!-- Billing Dates, Usage Dates, Terms, Reference Number-->
  <div class="rpt-flexcolumn" style="flex:0 0 321px;border-top: 2px solid #027cc5;padding:1px;margin-left:10px;">
    <table>
      <tr>
        <td style="width:105px;" class="shaded-block">Billing Period: </td>
        <td class="meta-content">{{BillingDates}}</td>
      </tr>
      <tr>
        <td style="width:105px;" class="shaded-block">Usage Period: </td>
        <td class="meta-content">{{UsageDates}}</td>
      </tr>
      <tr>
        <td style="width:105px;" class="shaded-block">Terms: </td>
        <td class="meta-content">{{PaymentTerms}}</td>
      </tr>
      {{#if ReferenceNumber}}
      <tr>
        <td style="width:105px;" class="shaded-block">Reference No: </td>
        <td class="meta-content">{{ReferenceNumber}}</td>
      </tr>
      {{/if}}
    </table>


  </div>
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  <!-- Billed To -->
  <div class="rpt-flexcolumn" style="flex:0 0 422px;margin-right:10px;">
    <table>
      <tr>
        <th class="sub text" style="text-align:left;padding:5px;background-color:#e3f2fd;">Billed To</th>
