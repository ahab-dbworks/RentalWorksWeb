<!--RateUpdateReport-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>

<!--RENTAL-->
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    {{#each RentalItems}}
    {{#if @first}}
    <tr><td>&nbsp;</td></tr><!-- spacer row -->
    <tr>
      <td colspan="11" class="section-header">RENTAL&nbsp;</td>
    </tr>
    {{/if}}
    <tbody>
      {{#ifEquals RowType "Warehouseheader"}}
      <tr data-rowtype={{RowType}}>
        <td colspan="3" class="header-row">Warehouse: {{Warehouse}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "InventoryTypeheader"}}
      <tr data-rowtype={{RowType}}>
        <td colspan="3" class="lower header-row">Type: {{InventoryType}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "Categoryheader"}}
      <tr data-rowtype={{RowType}}>
        <td colspan="3" class="lower header-row">Category: {{Category}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifEquals RowType "AvailableForDisplayheader"}}
      <tr class="top-section-header">
        <td colspan="4" style="background-color:white;"></td>
        <td style="border-right:3px solid black;">Daily Rate</td>
        <td style="border-right:3px solid black;">Weekly Rate</td>
        {{#if ThreeWeekEnabled}}
        <td style="border-right:3px solid black;">Week 2 Rate</td>
        <td style="border-right:3px solid black;">Week 3 Rate </td>
        <td style="border-right:3px solid black;">Week 4 Rate </td>
        {{/if}}
        <td style="border-right:3px solid black;">Monthly Rate</td>
        <td style="border-right:3px solid black;">Replacement Cost</td>
        <td style="border-right:3px solid black;">Unit Value</td>
        <td style="border-right:3px solid black;">Max Discount</td>
        <td>Min D/W</td>
      </tr>

      <tr>
        <td class="row1">I-Code</td>
        <td class="row1">Description</td>
        <td class="row1">Mfg. Part No</td>
        <td class="row1">Rank</td>
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
        {{#if ThreeWeekEnabled}}
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
        {{/if}}
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
        <td class="row1 number">Current</td>
      </tr>
      <tr>
        <td class="row2"></td>
        <td class="row2"></td>
        <td class="row2"></td>
        <td class="row2"></td>
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
        {{#if ThreeWeekEnabled}}
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
        {{/if}}
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
        <td class="row2 number">New</td>
      </tr>
      {{/ifEquals}}

      {{#ifEquals RowType "detail"}}
      <tr data-rowtype={{RowType}}>
        <td class="nowrap">{{ICode}}</td>
        <td data-value="{{Description}}"></td>
        <td data-value="{{PartNumber}}"></td>
        <td data-value="{{Rank}}"></td>
        <td>
          <div class="number" data-value="{{OldDailyRate}}"></div>
          <div class="number" data-value="{{NewDailyRate}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldWeeklyRate}}"></div>
          <div class="number" data-value="{{NewWeeklyRate}}"></div>
        </td>
        {{#if ThreeWeekEnabled}}
        <td>
          <div class="number" data-value="{{OldWeek2Rate}}"></div>
          <div class="number" data-value="{{NewWeek2Rate}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldWeek3Rate}}"></div>
          <div class="number" data-value="{{NewWeek3Rate}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldWeek4Rate}}"></div>
          <div class="number" data-value="{{NewWeek4Rate}}"></div>
        </td>
        {{/if}}
        <td>
          <div class="number" data-value="{{OldMonthlyRate}}"></div>
          <div class="number" data-value="{{NewMonthlyRate}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldReplacementCost}}"></div>
          <div class="number" data-value="{{NewReplacementCost}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldUnitValue}}"></div>
          <div class="number" data-value="{{NewUnitValue}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldMaxDiscount}}"></div>
          <div class="number" data-value="{{NewMaxDiscount}}"></div>
        </td>
        <td>
          <div class="number" data-value="{{OldMinDaysPerWeek}}"></div>
          <div class="number" data-value="{{NewMinDaysPerWeek}}"></div>
        </td>
      </tr>
      {{/ifEquals}}
    </tbody>
    {{/each}}
  </table>
</div>

