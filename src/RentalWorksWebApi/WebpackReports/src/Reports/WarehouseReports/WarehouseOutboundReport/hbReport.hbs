<!--WarehouseOutboundReport-->
<div class="rpt-flexrow">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:40px;">
  <table>
    <thead>
      <tr>
        <th>Pick Date</th>
        <th>Start Date</th>
        <th>Order Type</th>
        <th>Activity</th>
        <th>Out Type</th>
        <th>Order No.</th>
        <th>Order Status</th>
        <th>Deal</th>
      </tr>
      <tr>
        <th>Activity Status</th>
        <th>Assigned To</th>
        <th class="">Staged</th>
        <th class="">Items</th>
        <th class="">Remaining</th>
        <th class="">%</th>
        <th>Load Date</th>
        <th>Outgoing Address</th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "Warehouseheader"}}
        <td colspan="8" class="header-row">Warehouse: {{Warehouse}}</td>
        {{/ifEquals}}
        {{#ifEquals RowType "detail"}}
        <td data-value="{{PickDate}}"></td>
        <td data-value="{{ActivityDateTime}}"></td>
        <td data-value="{{OrderTypeDescription}}"></td>
        <td data-value="{{ActivityType}}"></td>
        <td data-value="{{ActivityTypeDescription}}"></td>
        <td data-value="{{OrderNumber}}"></td>
        <td data-value="{{OrderStatus}}"></td>
        <td data-value="{{Deal}}"></td>
        {{/ifEquals}}
      </tr>
      <tr data-rowtype={{RowType}}>
        {{#ifEquals RowType "detail"}}
        <td data-value="{{ActivityStatusDescription}}"></td>
        <td data-value="{{AssignedToUserName}}"></td>
        <td class="" data-value="{{TotalQuantity}}"></td>
        <td class="" data-value="{{CompleteQuantity}}"></td>
        <td class="" data-value="{{RemainingQuantity}}"></td>
        <td class="" data-value="{{CompletePercent}}"></td>
        <td data-value="{{LoadInDateTime}}"></td>
        <td data-value="{{OutgoingLocation}}"></td>
        {{/ifEquals}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>