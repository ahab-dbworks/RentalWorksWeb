<!--DealInvoiceDetailReport-->
<div data-section="header" class="rpt-flexrow" style="width:1555px;">
  <div class="rpt-flexcolumn">
    <span class="rpt-header-left">{{System}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-title">{{Report}}</span>
    <span class="rpt-header-center">From: {{FromDate}} To: {{ToDate}}</span>
  </div>
  <div class="rpt-flexcolumn">
    <span class="rpt-header-right">{{Company}}</span>
  </div>
</div>
<div data-section="table" class="rpt-flexrow" style="margin-top:40px;width:1555px;">
  <table>

    <thead id="columnHeader">
      <tr data-row="main-header" data-linkedrow="detail">
        <th data-linkedcolumn="OrderNumber" data-valuefield="OrderNumber"></th>
        <th data-linkedcolumn="OrderDate" data-valuefield="OrderDate"></th>
        <th data-linkedcolumn="OrderDescription" data-valuefield="OrderDescription" colspan="3"></th>
        <th data-linkedcolumn="BillingPeriod" data-valuefield="BillingPeriod"></th>
        <th data-linkedcolumn="EstimatedStartDate" data-valuefield="EstimatedStartDate"></th>
        <th data-linkedcolumn="EstimatedStopDate" data-valuefield="EstimatedStopDate"></th>
        <th data-linkedcolumn="BillingPeriodStart" data-valuefield="BillingPeriodStart"></th>
        <th data-linkedcolumn="BillingPeriodEnd" data-valuefield="BillingPeriodEnd"></th>
        <th data-linkedcolumn="EmptyTd-A1" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A2" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A3" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A4" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A5" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A6" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A7" data-valuefield=""></th>
        <th data-linkedcolumn="EmptyTd-A8" data-valuefield=""></th>
      </tr>
    </thead>
    <tbody>
      {{#each this}}

      {{#ifEquals RowType "OfficeLocationheader"}}
      <tr data-row="header" data-rowtype="{{RowType}}">
        <td colspan="18" class="header-row no-border">Office Location: {{OfficeLocation}}</td>
      </tr>
      {{/ifEquals}}

      {{#ifEquals RowType "Dealheader"}}
      <tr data-row="header" data-rowtype="{{RowType}}">
        <td colspan="18" class="header-row no-border">Deal: {{Deal}}</td>
      </tr>
      {{/ifEquals}}

      {{#ifEquals RowType "OrderNumberheader"}}
      <tr data-row="sub-header" data-sort="true" data-rowtype="{{RowType}}">
        <th class="sub-header" data-linkedcolumn="OrderNumber" data-valuefield="OrderNumber">Order No.</th>
        <th class="sub-header" data-linkedcolumn="OrderDate" data-valuefield="OrderDate">Date</th>
        <th class="sub-header" data-linkedcolumn="OrderDescription" data-valuefield="OrderDescription" colspan="3">Description</th>
        <th class="sub-header" data-linkedcolumn="BillingPeriod" data-valuefield="BillingPeriod">Billing Cycle</th>
        <th class="sub-header nowrap" data-linkedcolumn="EstimatedStartDate" data-valuefield="EstimatedStartDate">Est. Start Date</th>
        <th class="sub-header nowrap" data-linkedcolumn="EstimatedStopDate" data-valuefield="EstimatedStopDate">Est. Stop Date</th>
        <th class="sub-header nowrap" data-linkedcolumn="BillingPeriodStart" data-valuefield="BillingPeriodStart">Bill Start Date</th>
        <th class="sub-header nowrap" data-linkedcolumn="BillingPeriodEnd" data-valuefield="BillingPeriodEnd">Bill Stop Date</th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A1" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A2" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A3" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A4" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A5" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A6" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A7" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="EmptyTd-A8" data-valuefield=""></th>
      </tr>
      <tr data-row="sub-detail" data-rowtype="{{RowType}}">
        <td data-linkedcolumn="OrderNumber" data-value="{{OrderNumber}}"></td>
        <td data-linkedcolumn="OrderDate" data-value="{{OrderDate}}"></td>
        <td data-linkedcolumn="OrderDescription" data-value="{{OrderDescription}}" colspan="3"></td>
        <td data-linkedcolumn="BillingPeriod" data-value="{{BillingPeriod}}"></td>
        <td data-linkedcolumn="EstimatedStartDate" data-value="{{EstimatedStartDate}}"></td>
        <td data-linkedcolumn="EstimatedStopDate" data-value="{{EstimatedStopDate}}"></td>
        <td data-linkedcolumn="BillingPeriodStart" data-value="{{BillingPeriodStart}}"></td>
        <td data-linkedcolumn="BillingPeriodEnd" data-value="{{BillingPeriodEnd}}"></td>
        <td data-linkedcolumn="EmptyTd-A1"></td>
        <td data-linkedcolumn="EmptyTd-A2"></td>
        <td data-linkedcolumn="EmptyTd-A3"></td>
        <td data-linkedcolumn="EmptyTd-A4"></td>
        <td data-linkedcolumn="EmptyTd-A5"></td>
        <td data-linkedcolumn="EmptyTd-A6"></td>
        <td data-linkedcolumn="EmptyTd-A7"></td>
        <td data-linkedcolumn="EmptyTd-A8"></td>
      </tr>
      <tr data-row="linked-sub-header" data-sort="true" data-linkedrow="detail" data-rowtype="{{RowType}}">
        <th class="" data-linkedcolumn="EmptyTd-B1" data-valuefield=""></th>
        <th class="sub-header" data-linkedcolumn="InvoiceNumber" data-valuefield="InvoiceNumber">Invoice No.</th>
        <th class="sub-header" data-linkedcolumn="InvoiceDate" data-valuefield="InvoiceDate">Inv. Date</th>
        <th class="sub-header" data-linkedcolumn="BillingStartDate" data-valuefield="BillingStartDate">Bill Start</th>
        <th class="sub-header" data-linkedcolumn="BillingStopDate" data-valuefield="BillingStopDate">Bill Stop</th>
        <th class="sub-header" data-linkedcolumn="Status" data-valuefield="Status">Status</th>
        <th class="sub-header" data-linkedcolumn="NoChargeDescription" data-valuefield="NoChargeDescription">No Charge</th>
        <th class="sub-header" data-linkedcolumn="HiatusDescription" data-valuefield="HiatusDescription">Hiatus</th>
        <th class="sub-header" data-linkedcolumn="FlatPODescription" data-valuefield="FlatPODescription">Flat</th>
        <th class="sub-header number" data-linkedcolumn="RentalRevenue" data-valuefield="RentalRevenue">Rental</th>
        <th class="sub-header number" data-linkedcolumn="SalesRevenue" data-valuefield="SalesRevenue">Sales</th>
        <th class="sub-header number" data-linkedcolumn="SpaceRevenue" data-valuefield="SpaceRevenue">Facilities</th>
        <th class="sub-header number" data-linkedcolumn="LaborRevenue" data-valuefield="LaborRevenue">Labor</th>
        <th class="sub-header number" data-linkedcolumn="MiscRevenue" data-valuefield="MiscRevenue">Misc.</th>
        <th class="sub-header number" data-linkedcolumn="RentalSaleRevenue" data-valuefield="RentalSaleRevenue">Asset</th>
        <th class="sub-header number" data-linkedcolumn="PartsRevenue" data-valuefield="PartsRevenue">Parts</th>
        <th class="sub-header number" data-linkedcolumn="Tax" data-valuefield="Tax">Tax</th>
        <th class="sub-header number" data-linkedcolumn="TotalRevenue" data-valuefield="TotalRevenue">Total</th>
      </tr>
      {{/ifEquals}}

      {{#ifEquals RowType "detail"}}
      <tr id="detailRowInvoice" class="detail" data-row="detail" data-linkedrow="detail" data-rowtype="{{RowType}}">
        <td data-linkedcolumn="EmptyTd-B1" data-value=""></td>
        <td data-linkedcolumn="InvoiceNumber" data-value="{{InvoiceNumber}}"></td>
        <td data-linkedcolumn="InvoiceDate" data-value="{{InvoiceDate}}"></td>
        <td data-linkedcolumn="BillingStartDate" data-value="{{BillingStartDate}}"></td>
        <td data-linkedcolumn="BillingStopDate" data-value="{{BillingStopDate}}"></td>
        <td data-linkedcolumn="Status" data-value="{{Status}}"></td>
        <td data-linkedcolumn="NoChargeDescription" data-value="{{NoChargeDescription}}"></td>
        <td data-linkedcolumn="HiatusDescription" data-value="{{HiatusDescription}}"></td>
        <td data-linkedcolumn="FlatPODescription" data-value="{{FlatPODescription}}"></td>
        <td data-linkedcolumn="RentalRevenue" class="number" data-value="{{RentalRevenue}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="number" data-value="{{SalesRevenue}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="number" data-value="{{SpaceRevenue}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="number" data-value="{{LaborRevenue}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="number" data-value="{{MiscRevenue}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="number" data-value="{{RentalSaleRevenue}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="number" data-value="{{PartsRevenue}}"></td>
        <td data-linkedcolumn="Tax" class="number" data-value="{{Tax}}"></td>
        <td data-linkedcolumn="TotalRevenue" class="number" data-value="{{TotalRevenue}}"></td>
      </tr>
      {{#if DeductVendorItemCost}}
      {{#if TotalCost}}
      <tr id="detailRowCost" class="detail" data-row="detail" data-linkedrow="detail" data-rowtype="{{RowType}}">
        <td data-linkedcolumn="EmptyTd-B1" data-value=""></td>
        <td data-linkedcolumn="InvoiceNumber" data-value=""></td>
        <td data-linkedcolumn="InvoiceDate" data-value=""></td>
        <td data-linkedcolumn="BillingStartDate" data-value=""></td>
        <td data-linkedcolumn="BillingStopDate" data-value=""></td>
        <td data-linkedcolumn="Status" data-value=""></td>
        <td data-linkedcolumn="NoChargeDescription" data-value=""></td>
        <td data-linkedcolumn="HiatusDescription" data-value=""></td>
        <td data-linkedcolumn="FlatPODescription" data-value="">Cost of Subs:&nbsp;</td>
        <td data-linkedcolumn="RentalRevenue" class="number" data-value="{{RentalCost}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="number" data-value="{{SalesCost}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="number" data-value="{{SpaceCost}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="number" data-value="{{LaborCost}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="number" data-value="{{MiscCost}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="number" data-value="{{RentalSaleCost}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="number" data-value="{{PartsCost}}"></td>
        <td data-linkedcolumn="Tax" class="number"></td>
        <td data-linkedcolumn="TotalRevenue" class="number" data-value="{{TotalCost}}"></td>
      </tr>
      <tr id="detailRowNet" class="detail" data-row="detail" data-linkedrow="detail" data-rowtype="{{RowType}}">
        <td data-linkedcolumn="EmptyTd-B1" data-value=""></td>
        <td data-linkedcolumn="InvoiceNumber" data-value=""></td>
        <td data-linkedcolumn="InvoiceDate" data-value=""></td>
        <td data-linkedcolumn="BillingStartDate" data-value=""></td>
        <td data-linkedcolumn="BillingStopDate" data-value=""></td>
        <td data-linkedcolumn="Status" data-value=""></td>
        <td data-linkedcolumn="NoChargeDescription" data-value=""></td>
        <td data-linkedcolumn="HiatusDescription" data-value=""></td>
        <td data-linkedcolumn="FlatPODescription" data-value="">Net:&nbsp;</td>
        <td data-linkedcolumn="RentalRevenue" class="number" data-value="{{RentalNet}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="number" data-value="{{SalesNet}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="number" data-value="{{SpaceNet}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="number" data-value="{{LaborNet}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="number" data-value="{{MiscNet}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="number" data-value="{{RentalSaleNet}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="number" data-value="{{PartsNet}}"></td>
        <td data-linkedcolumn="Tax" class="number"></td>
        <td data-linkedcolumn="TotalRevenue" class="number" data-value="{{TotalNet}}"></td>
      </tr>
      {{/if}}
      {{/if}}
      {{/ifEquals}}

      {{#ifEquals RowType "OrderNumberfooter"}}
      <tr data-row="footer" data-rowtype="{{RowType}}">
        <td colspan="9" class="total-name">{{OrderNumber}}:&nbsp;</td>
        <td data-linkedcolumn="RentalRevenue" class="total-val" data-value="{{RentalNet}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="total-val" data-value="{{SalesNet}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="total-val" data-value="{{SpaceNet}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="total-val" data-value="{{LaborNet}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="total-val" data-value="{{MiscNet}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="total-val" data-value="{{RentalSaleNet}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="total-val" data-value="{{PartsNet}}"></td>
        <td data-linkedcolumn="Tax" class="total-val" data-value="{{Tax}}"></td>
        <td data-linkedcolumn="TotalRevenue" class="total-val" data-value="{{TotalNet}}"></td>
      </tr>
      {{/ifEquals}}


      {{#ifEquals RowType "Dealfooter"}}
      <tr data-row="footer" data-rowtype="{{RowType}}">
        <td colspan="9" class="total-name">{{Deal}}:&nbsp;</td>
        <td data-linkedcolumn="RentalRevenue" class="total-val" data-value="{{RentalNet}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="total-val" data-value="{{SalesNet}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="total-val" data-value="{{SpaceNet}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="total-val" data-value="{{LaborNet}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="total-val" data-value="{{MiscNet}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="total-val" data-value="{{RentalSaleNet}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="total-val" data-value="{{PartsNet}}"></td>
        <td data-linkedcolumn="Tax" class="total-val" data-value="{{Tax}}"></td>
        <td data-linkedcolumn="TotalRevenue" class="total-val" data-value="{{TotalNet}}"></td>
      </tr>
      {{/ifEquals}}

      {{#ifEquals RowType "OfficeLocationfooter"}}
      <tr data-row="footer" data-rowtype="{{RowType}}">
        <td colspan="9" class="total-name">{{OfficeLocation}}:&nbsp;</td>
        <td data-linkedcolumn="RentalRevenue" class="total-val" data-value="{{RentalNet}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="total-val" data-value="{{SalesNet}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="total-val" data-value="{{SpaceNet}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="total-val" data-value="{{LaborNet}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="total-val" data-value="{{MiscNet}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="total-val" data-value="{{RentalSaleNet}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="total-val" data-value="{{PartsNet}}"></td>
        <td data-linkedcolumn="Tax" class="total-val" data-value="{{Tax}}"></td>
        <td data-linkedcolumn="TotalRevenue" class="total-val" data-value="{{TotalNet}}"></td>
      </tr>
      {{/ifEquals}}


      {{#ifEquals RowType "grandtotal"}}
      <tr data-row="footer" data-rowtype="{{RowType}}">
        <td colspan="9" class="total-name">Grand Total:&nbsp;</td>
        <td data-linkedcolumn="RentalRevenue" class="total-val" data-value="{{RentalNet}}"></td>
        <td data-linkedcolumn="SalesRevenue" class="total-val" data-value="{{SalesNet}}"></td>
        <td data-linkedcolumn="SpaceRevenue" class="total-val" data-value="{{SpaceNet}}"></td>
        <td data-linkedcolumn="LaborRevenue" class="total-val" data-value="{{LaborNet}}"></td>
        <td data-linkedcolumn="MiscRevenue" class="total-val" data-value="{{MiscNet}}"></td>
        <td data-linkedcolumn="RentalSaleRevenue" class="total-val" data-value="{{RentalSaleNet}}"></td>
        <td data-linkedcolumn="PartsRevenue" class="total-val" data-value="{{PartsNet}}"></td>
        <td data-linkedcolumn="Tax" class="total-val" data-value="{{Tax}}"></td>
        <td data-linkedcolumn="TotalRevenue" class="total-val" data-value="{{TotalNet}}"></td>
      </tr>
      {{/ifEquals}}


      {{/each}}
  </table>
</div>
