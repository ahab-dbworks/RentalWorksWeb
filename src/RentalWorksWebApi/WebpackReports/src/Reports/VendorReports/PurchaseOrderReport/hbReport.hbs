      <tr>
        <th class="sub text" style="text-align:left;padding:5px;background-color:#e3f2fd;">Return to Vendor ({{InDeliveryType}})</th>
      </tr>
      <tr>
        <td class=""><span style="font-weight:700;">Location: </span>{{InDeliveryLocation}}</td>
      </tr>
      {{#ifCond InDeliveryAttention '!==' ""}}
      <tr>
        <td class=""><span style="font-weight:700;">Attention: </span>{{InDeliveryAttention}}</td>
      </tr>
      {{/ifCond}}
      <tr>
        <td class="">{{InDeliveryAddress1}}</td>
      </tr>
      {{#ifCond InDeliveryAddress2 '!==' ""}}
      <tr>
        <td class="">{{InDeliveryAddress2}}</td>
      </tr>
      {{/ifCond}}
      {{#ifCond InDeliveryCity '!==' ""}}
      <tr>
        <td class="">{{InDeliveryCity}}, {{InDeliveryState}} {{InDeliveryZipCode}} {{InDeliveryCountry}}</td>
      </tr>
      {{/ifCond}}
      {{#ifCond InDeliveryRequiredByDateTime '!==' ""}}
      <tr>
        <td class=""><span style="font-weight:700;">Required By: </span>{{InDeliveryRequiredByDateTime}}</td>
      </tr>
      {{/ifCond}}
    </table>
    {{/ifNotEquals}}
  </div>
</div>

