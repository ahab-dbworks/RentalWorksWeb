<!-- Purchase Order Report-->
<div class="rpt-flexrow" style="margin:5px 0px 0px 4px;">
  <div class="rpt-flexcolumn" style="flex:0 0 30%;padding-bottom:5px;">
    {{#if Logosrc}}
    <div style="height:100px;"><img class="clientlogo" src="{{Logosrc}}" style="display:block;width:auto;height:100%;" alt="logo" /></div>
    {{else}}
    <div></div>
    {{/if}}
    <span class="header-left" style="font-weight:600">{{OfficeLocationCompany}}</span>
    <span class="header-left">{{OfficeLocationAddress1}}</span>
    <span class="header-left">{{OfficeLocationAddress2}}</span>
    <span class="header-left">{{OfficeLocationCityStateZipCodeCountry}}</span>
    <span class="header-left"><span style="font-weight:600;">Phone: </span>{{OfficeLocationPhone}}</span>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 40%;">
    <span class="sub-title">{{Department}}</span>
    <span class="title blue">{{Report}}</span>
  </div>
  <!-- Order Number & Date -->
  <div class="rpt-flexcolumn" style="flex:0 0 30%;">
    <table>
      <tr>
        <td style="width:50%;font-size:18px;font-weight:700;text-align:right;">No:</td>
        <td style="width:50%;background-color:#e3f2fd;color:#027cc5;border:2px solid #212121;padding:5px 10px;font-size:26px;text-align:center;">{{OrderNumber}}</td>
      </tr>
      <tr>
        <td style="width:50%;color:black;font-size:18px;font-weight:700;text-align:right;">Date:</td>
        <td style="width:50%;font-size:18px;text-align:center;">{{OrderDate}}</td>
      </tr>
      {{#ifCond OutDeliveryRequiredByDateTime '!==' ""}}
      <tr>
        <td style="width:50%;color:black;font-size:18px;font-weight:700;text-align:right;"></td>
        <td style="width:50%;font-size:18px;text-align:center;padding-top:10px;"><span style="color:black;font-size:18px;font-weight:700;text-align:right;">Required By: </span>{{OutDeliveryRequiredByDateTime}}</td>
      </tr>
      {{/ifCond}}
    </table>
  </div>
</div>

<div class="rpt-flexrow" style="margin-top:15px;">
  <div class="rpt-flexcolumn" style="flex:0 0 400px;margin-right:10px;">
    <!-- Issued To -->
    <table id="issuedTo">
      <tr>
        <th class="sub text" style="text-align:left;padding:5px;background-color:#e3f2fd;">Issued To</th>
      </tr>
      <tr>
        <td class="" style="font-weight:700;">{{IssuedToCompany}}</td>
      </tr>
      <tr>
        <td class="">{{IssuedToAddress1}}</td>
      </tr>
      {{#ifCond IssuedToAddress2 '!==' ""}}
      <tr>
        <td class="">{{IssuedToAddress2}}</td>
      </tr>
      {{/ifCond}}
      <tr>
        <td class="">{{IssuedToCity}}, {{IssuedToState}} {{IssuedToZipCode}} {{IssuedToCountry}}</td>
      </tr>
      <tr>
        <td class=""><span style="color:#000000;font-weight:700;">Phone: </span>{{IssuedToPhone}}</td>
      </tr>
      <tr>
        <td class=""><span style="color:#000000;font-weight:700;">Fax: </span><span style="padding-left:20px;">{{IssuedToFax}}</span></td>
      </tr>
    </table>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 332px;border-top: 2px solid #027cc5;margin:0px 10px;">
    <!-- Ordered From -->
    <table>
      <tr>
        <td style="width:110px;background-color:#e3f2fd;font-weight:700;">Ordered From:</td>
        <td class="">{{OrderedBy}}</td>
      </tr>
      <tr>
        <td style="width:110px;background-color:#e3f2fd;font-weight:700;">Phone:</td>
        <td class="">{{OrderedByPhone}}</td>
      </tr>
      <tr>
        <td style="width:110px;background-color:#e3f2fd;font-weight:700;">Email:</td>
        <td class="">{{OrderedByEmail}}</td>
      </tr>
    </table>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 332px;margin-left:10px;">
    <!-- Receive from Vendor -->
    <table id="outgoing">
      <tr>
        <th class="sub text" style="text-align:left;padding:5px;background-color:#e3f2fd;">Receive from Vendor ({{OutDeliveryType}})</th>
      </tr>
      <tr>
        <td class=""><span style="font-weight:700;">Location: </span>{{OutDeliveryLocation}}</td>
      </tr>
      {{#ifCond OutDeliveryAttention '!==' ""}}
      <tr>
        <td class=""><span style="font-weight:700;">Attention: </span>{{OutDeliveryAttention}}</td>
      </tr>
      {{/ifCond}}
      <tr>
        <td class="">{{OutDeliveryAddress1}}</td>
      </tr>
      {{#ifCond OutDeliveryAddress2 '!==' ""}}
      <tr>
        <td class="">{{OutDeliveryAddress2}}</td>
      </tr>
      {{/ifCond}}
      {{#ifCond OutDeliveryCity '!==' ""}}
      <tr>
        <td class="">{{OutDeliveryCity}}, {{OutDeliveryState}} {{OutDeliveryZipCode}} {{OutDeliveryCountry}}</td>
      </tr>
      {{/ifCond}}
      {{#ifCond OutDeliveryRequiredByDateTime '!==' ""}}
      <tr>
        <td class=""><span style="font-weight:700;">Required By: </span>{{OutDeliveryRequiredByDateTime}}</td>
      </tr>
      {{/ifCond}}
    </table>
  </div>
</div>

<!-- Ordered By  -->
<div class="rpt-flexrow" style="margin-top:25px;">
  <div class="rpt-flexcolumn" style="flex:0 0 400px;border-top: 2px solid #027cc5;padding:1px;margin-right:10px;">
    <!-- Description, Billing Period, Usage Period, etc -->
    <table>
      {{#ifEquals OrderTypeDescription "INVENTORY"}}
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Description:</td>
        <td class="">{{Description}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifNotEquals OrderTypeDescription "INVENTORY"}}
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Billing Period:</td>
        <td class="">{{BillingDates}}</td>
      </tr>
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Usage Period:</td>
        <td class="">{{UsageDates}}</td>
      </tr>
      {{/ifNotEquals}}
      <tr>
        <td style="width:105px;background-color:#e3f2fd;font-weight:700;">Terms:</td>
        <td class="">{{PaymentTerms}}</td>
      </tr>
      <tr>
        <td style="width:105px;background-color:#e3f2fd;;font-weight:700;">Billing Cycle:</td>
        <td class="">{{BillingCycle}}</td>
      </tr>
    </table>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 332px;padding:1px;border-top: 2px solid #027cc5;margin:0px 10px;">
    <!-- Ordered By -->
    <table>
      <tr>
        <td style="width:110px;background-color:#e3f2fd;font-weight:700;">Ordered By:</td>
        <td class="">{{Agent}}</td>
      </tr>
      <tr>
        <td style="width:110px;background-color:#e3f2fd;font-weight:700;">Phone:</td>
        <td class="">{{AgentPhone}}</td>
      </tr>
      <tr>
        <td style="width:110px;background-color:#e3f2fd;font-weight:700;">Email:</td>
        <td class="">{{AgentEmail}}</td>
      </tr>
    </table>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 332px;padding:1px;margin-left:10px;">
    <!-- Return to Vendor -->
    {{#ifNotEquals OrderTypeDescription "INVENTORY"}}
