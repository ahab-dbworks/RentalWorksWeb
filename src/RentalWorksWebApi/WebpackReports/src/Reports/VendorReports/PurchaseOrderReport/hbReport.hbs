<!-- Purchase Order Report-->
<div class="rpt-flexrow" style="margin:5px 0px 0px 4px;">
  <div class="rpt-flexcolumn" style="flex:0 0 30%;padding-bottom:5px;">
    {{#if Logosrc}}
    <div style="height:100px;"><img class="clientlogo" src="{{Logosrc}}" style="display:block;width:auto;height:100%;" alt="logo" /></div>
    {{else}}
    <div></div>
    {{/if}}
    <span class="header-left" style="font-weight:600">{{OfficeLocationCompany}}</span>
    <span class="header-left">{{OfficeLocationAddress1}}</span>
    <span class="header-left">{{OfficeLocationAddress2}}</span>
    <span class="header-left">{{OfficeLocationCityStateZipCodeCountry}}</span>
    <span class="header-left"><span style="font-weight:600;">Phone: </span>{{OfficeLocationPhone}}</span>
  </div>
  <div class="rpt-flexcolumn" style="flex:0 0 40%;">
    <span class="sub-title">{{Department}}</span>
    <span class="title">{{Report}}</span>
  </div>
  <!-- Order Number & Date -->
  <div class="rpt-flexcolumn" style="flex:0 0 30%;">
    <table>
      <tr>
        <td style="width:50%;font-size:18px;font-weight:700;text-align:right;">No:</td>
        <td class="rptNumberSection" style="width:50%;">{{OrderNumber}}</td>
      </tr>
      <tr>
        <td style="width:50%;color:black;font-size:18px;font-weight:700;text-align:right;">Date:</td>
        <td style="width:50%;font-size:18px;text-align:center;">{{OrderDate}}</td>
      </tr>
    </table>
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:15px;">
  <!-- Issued To -->
  <div class="rpt-flexcolumn" style="flex:0 0 400px;margin-right:10px;">
    <table id="issuedTo">
      <tr>
        <th class="sub text shaded-block" style="text-align:left;padding:5px;">Issued To</th>
      </tr>
      <tr>
        <td class="" style="font-weight:700;">{{IssuedToCompany}}</td>
      </tr>
      <tr>
        <td class="">{{IssuedToAddress1}}</td>
      </tr>
      {{#ifCond IssuedToAddress2 '!==' ""}}
      <tr>
        <td class="">{{IssuedToAddress2}}</td>
      </tr>
      {{/ifCond}}
      <tr>
        <td class="">{{IssuedToCity}}, {{IssuedToState}} {{IssuedToZipCode}} {{IssuedToCountry}}</td>
      </tr>
      <tr>
        <td class=""><span style="color:#000000;font-weight:700;">Phone: </span>{{IssuedToPhone}}</td>
      </tr>
    </table>
  </div>
  <!-- Ordered From -->
  <div class="rpt-flexcolumn sectionBorder" style="flex:0 0 332px;margin:0px 10px;">
    <table>
      {{#if OrderedBy}}
      <tr>
        <td class="shaded-block" style="width:110px;">Ordered From:</td>
        <td class="">{{OrderedBy}}</td>
      </tr>
      {{/if}}
      {{#if OrderedByEmail}}
      <tr>
        <td class="shaded-block" style="width:110px;">Email:</td>
        <td class="">{{OrderedByEmail}}</td>
      </tr>
      {{/if}}
      {{#if OrderedByPhone}}
      <tr>
        <td class="shaded-block" style="width:110px;">Phone:</td>
        <td class="">{{OrderedByPhone}}</td>
      </tr>
      {{/if}}
    </table>
  </div>
  <!-- Receive from Vendor -->
  <div class="rpt-flexcolumn " style="flex:0 0 332px;margin-left:10px;">
    {{#ifNotEquals OrderTypeDescription "MISCELLANEOUS"}}
    <table id="outgoing">
      <tr>
        <th class="sub text shaded-block" style="text-align:left;padding:5px;">Receive from Vendor ({{OutDeliveryType}})</th>
      </tr>
      <tr>
        <td class=""><span style="font-weight:700;">Location: </span>{{OutDeliveryLocation}}</td>
      </tr>
      {{#if OutDeliveryAttention}}
      <tr>
        <td class=""><span style="font-weight:700;">Attention: </span>{{OutDeliveryAttention}}</td>
      </tr>
      {{/if}}
      <tr>
        <td class="">{{OutDeliveryAddress1}}{{#if OutDeliveryAddress2}},&nbsp;{{OutDeliveryAddress2}}{{/if}}</td>
      </tr>
      {{#if OutDeliveryCity}}
      <tr>
        <td class="">{{OutDeliveryCity}}, {{OutDeliveryState}} {{OutDeliveryZipCode}} {{OutDeliveryCountry}}</td>
      </tr>
      {{/if}}
      {{#if OutDeliveryRequiredByDateTime}}
      <tr>
        <td class=""><span style="font-weight:700;">Required By: </span>{{OutDeliveryRequiredByDateTime}}</td>
      </tr>
      {{/if}}
    </table>
    {{/ifNotEquals}}
  </div>
</div>
<div class="rpt-flexrow" style="margin-top:25px;">
  <!-- Description, Billing Period, Usage Period, etc -->
  <div class="rpt-flexcolumn sectionBorder" style="flex:0 0 400px;margin-right:10px;">
    <table>
      {{#ifEquals OrderTypeDescription "INVENTORY"}}
      <tr>
        <td class="shaded-block" style="width:105px;">Description:</td>
        <td class="">{{Description}}</td>
      </tr>
      {{/ifEquals}}
      {{#ifNotEquals OrderTypeDescription "INVENTORY"}}
      {{#ifCond BillingDates '!==' "N/A"}}
      <tr>
        <td class="shaded-block" style="width:105px;">Billing Period:</td>
        <td class="">{{BillingDates}}</td>
      </tr>
      {{/ifCond}}
      {{#ifCond UsageDates '!==' "N/A"}}
      <tr>
        <td class="shaded-block" style="width:105px;">Usage Period:</td>
        <td class="">{{UsageDates}}</td>
      </tr>
      {{/ifCond}}
      {{/ifNotEquals}}
      <tr>
        <td class="shaded-block" style="width:105px;">Terms:</td>
        <td class="">{{PaymentTerms}}</td>
      </tr>
      <tr>
        <td class="shaded-block" style="width:105px;">Billing Cycle:</td>
        <td class="">{{BillingCycle}}</td>
      </tr>
    </table>
  </div>
  <!-- Ordered By -->
  <div class="rpt-flexcolumn sectionBorder" style="flex:0 0 332px;margin:0px 10px;">
    <table>
      <tr>
        <td class="shaded-block" style="width:110px;">Ordered By:</td>
        <td class="">{{Agent}}</td>
      </tr>
      <tr>
        <td class="shaded-block" style="width:110px;">Email:</td>
        <td class="">{{AgentEmail}}</td>
      </tr>
      <tr>
        <td class="shaded-block" style="width:110px;">Phone:</td>
        <td class="">{{AgentPhone}}</td>
      </tr>
    </table>
  </div>
  <!-- Return to Vendor -->
  <div class="rpt-flexcolumn" style="flex:0 0 332px;margin-left:10px;">
    {{#ifNotEquals OrderTypeDescription "INVENTORY"}}
    {{#ifNotEquals OrderTypeDescription "MISCELLANEOUS"}}
    <table id="incoming">
      <tr>
        <th class="sub text shaded-block" style="text-align:left;padding:5px;background-color:#e3f2fd;">Return to Vendor ({{InDeliveryType}})</th>
      </tr>
      <tr>
        <td class=""><span style="font-weight:700;">Location: </span>{{InDeliveryLocation}}</td>
      </tr>
      {{#if InDeliveryAttention}}
      <tr>
        <td class=""><span style="font-weight:700;">Attention: </span>{{InDeliveryAttention}}</td>
      </tr>
      {{/if}}
      <tr>
        <td class="">{{InDeliveryAddress1}}{{#if InDeliveryAddress2}},&nbsp;{{InDeliveryAddress2}}{{/if}}</td>
      </tr>
      {{#if InDeliveryCity}}
      <tr>
        <td class="">{{InDeliveryCity}}, {{InDeliveryState}} {{InDeliveryZipCode}} {{InDeliveryCountry}}</td>
      </tr>
      {{/if}}
      {{#if InDeliveryRequiredByDateTime}}
      <tr>
        <td class=""><span style="font-weight:700;">Required By: </span>{{InDeliveryRequiredByDateTime}}</td>
      </tr>
      {{/if}}
    </table>
    {{/ifNotEquals}}
    {{/ifNotEquals}}
  </div>
</div>
<!-- Rental Items -->
