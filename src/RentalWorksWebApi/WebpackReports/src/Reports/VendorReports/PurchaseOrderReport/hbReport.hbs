      <td colspan="4"></td>
      <td colspan="2" class="display-footer" style="background-color:#e3f2fd">{{RecTypeDisplay}}&nbsp;</td>
      <td class="display-footer-total" data-value="{{PeriodExtended}}"></td>
    </tr>
    {{/ifEquals}}
    {{/each}}
  </table>
</div>

