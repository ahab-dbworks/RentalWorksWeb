{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ],
    "lib": [
      "dom",
      "es2015",
      "es5",
      "es6"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
