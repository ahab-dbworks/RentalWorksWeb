<JSAppBuilderConfig>
  <SchemaPath>FwApplicationSchema.config</SchemaPath>
  <DatabaseConnection>RentalWorks</DatabaseConnection>
  <Fields>
    <Field Publish="false" Key="{{FwJsonPath}}" Value="$(SolutionDir)\lib\Fw\src\Fw.Json\content" />
    <Field Publish="false" Key="{{FwJsonUri}}" Value="/rwfwjson" />
    <Field Publish="false" Key="{{AppPath}}" Value="$(SolutionDir)\src\RentalWorksWeb" />
    <Field Publish="false" Key="{{AppUri}}" Value="/rentalworksweb" />

    <Field Publish="true" Key="{{FwJsonPath}}" Value="$(SolutionDir)\lib\Fw\src\Fw.Json\content" />
    <Field Publish="true" Key="{{FwJsonUri}}" Value="http://localhost/rwfwjson" />
    <Field Publish="true" Key="{{AppPath}}" Value="$(SolutionDir)\src\RentalWorksWeb" />
    <Field Publish="true" Key="{{AppUri}}" Value="http://localhost/rentalworksweb" />
  </Fields>
  <MergeSections>
    <MergeSection>
      <ReplaceField>{{{applicationConfigVersion}}}</ReplaceField>
      <MergeFiles>
        <MergeFile>
          <Template>
            <![CDATA[<script type="text/javascript">
{{{File}}}
applicationConfig.version='{{Version}}';
</script>]]>
          </Template>
          <Uri>{{AppPath}}\scripts\DefaultApplicationConfig.js</Uri>
        </MergeFile>
      </MergeFiles>
    </MergeSection>
  </MergeSections>

