{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "group": "build",
            "type": "process",
            "options": {
                "cwd": "${workspaceFolder}/src/RentalWorksWebApi"
            },
            "args": [
                "build",
                "\"${workspaceFolder}/src/RentalWorksWebApi/WebApi.csproj\"",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "group": "build",
