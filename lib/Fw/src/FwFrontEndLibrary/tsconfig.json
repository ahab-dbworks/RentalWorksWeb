{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types/*"
    ],
    "types": [
      "jquery"
    ]
  },
  "include": [
    "./**/*"
    //"wwwroot/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "typeAcquisition": {"enable": true}
}
